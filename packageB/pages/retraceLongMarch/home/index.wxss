page{
  background: #EECDC8;
  background-image: linear-gradient(to bottom,#F8D5D3, #EECDC8,#EECDC8);
}
.wintermap-box{
  height: 422rpx;
  padding-top: 170rpx;
  padding-bottom: 10rpx;
  background-image: url('https://oss.qingshanpai.com/retraceLongMarch/r-bg.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  box-sizing: border-box;
}
.peple-box{
  width: 100%;
  padding: 20rpx 0;
  font-size: 32rpx;
  font-weight: 600;
  color: #ff0000;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-left: 160rpx;
  box-sizing: border-box;
}
.punchpepole-box{
  margin: 10rpx 24rpx;
  box-sizing: border-box;
  position: relative;
  opacity: 0.8;
}
.punchpepole-listbox{
  width: 100%;
  height: 62rpx;
  border-radius: 20px;
  overflow: hidden;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  padding: 0 12rpx;
  box-sizing: border-box;
  background: #fff;
}
.userList-number-img {
  max-width: 302rpx;
  height: 100%;
  position: relative;
  overflow: hidden;
}
.userList-number-img>view {
  border-radius: 50%;
  width: 48rpx;
  height: 48rpx;
  vertical-align: auto;
  overflow: hidden;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.userList-number-img>view>image {
  width: 100%;
  height: 100%;
}
.userList-text {
  flex: 1;
  font-size: 28rpx;
  font-weight: 600;
  text-align: left;
}
.punchmap-box{
  height: 512rpx;
  border-radius: 8px;
  background: #fff;
  margin: 0 22rpx;
  border: 1px solid #EECDC8;
  overflow: hidden;
}
.container-listbox{
  padding: 390rpx 24rpx 30rpx 24rpx;
  box-sizing: border-box;
  background-image: linear-gradient(to bottom,#F8D5D3, #EECDC8,#EECDC8);
} 
.playList-box{
  margin: 10rpx 0;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 6px;
  background: #fff;
  font-size: 30rpx;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
}
.playList-box-l{
  flex: 1;
  display: flex;
  align-items: flex-start;
}
.playList-box-r{
  width: 154rpx;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-direction: column;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.acList-l-avatar {
  width: 22rpx;
  height: 54rpx;
  margin-top: 8rpx;
}
.acList-l-view {
  padding-left: 12rpx;
}
.acList-l-title {
  font-size: 30rpx;
  font-weight: 600;
}
.acList-l-ac {
  font-size: 28rpx;
  padding-top: 8rpx;
  color: #999;
}
.acList-r-btn {
  width: 112rpx;
  height: 50rpx;
  line-height: 50rpx;
  position: relative;
  text-align: center;
  margin: 8rpx 0;
  border-radius: 20px;
  box-shadow: 0 3px 4px #E5061230;
  box-sizing: border-box;
}
.acList-r-btn.r-btn-t{
  background: #E50612;
  color: #fff;
}
.acList-r-btn.r-btn-b{
  background: #fff;
  color: #E50612;
  border: 1px solid #E50612;
  margin-top: 20rpx;
}
.ckgd-box{
  width: 256rpx;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  font-size: 30rpx;
  border-radius: 20px;
  background: #fff;
  box-shadow: 0 3px 4px #E5061230;
  margin: 30rpx auto;
  box-sizing: border-box;
}
.btnitems-box{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  padding: 30rpx 0 60rpx;
  box-sizing: border-box;
}
.btnitems-box>view,.btnitems-box>button{
  width: 144rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border-radius: 20px;
  color: #fff;
  background-image: linear-gradient(to right,#ff6c00,#fcb200);
  margin: 0 20rpx;
  box-shadow: 0 3px 5px #f1b052;
  font-size: 30rpx;
}
.hoverClass{
  opacity: 0.8;
}
.acList-box-r{
  width: 168rpx;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-direction: column;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.a-r-btn {
  width: auto;
  height: 54rpx;
  line-height: 54rpx;
  position: relative;
  text-align: center;
  margin: 8rpx 0;
  border-radius: 20px;
  box-shadow: 0 3px 4px #E5061230;
  box-sizing: border-box;
  font-size: 28rpx;
  padding: 0 16rpx;
}
.a-r-btn.a-btn-t{
  background: #E50612;
  color: #fff;
}

/* ===================  报名弹窗 ==================== */
.market-signup-box{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}
.market-signup-mark{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: #33333360;
  opacity: 0.6;
  filter: alpha(opacity=60);
}
.market-signup-infobox{
  position: fixed;
  top: 50%;
  left: 50%;
  width: 90%;
  height: auto;
  background: #fff;
  transform: translate(-50%, -50%);
  padding: 30rpx 10rpx;
  border-radius: 6px;
}
.hoverClass{
  opacity: 0.8;
  filter: alpha(opacity=80);
}
.tips {
  padding: 0 20rpx;
  position: relative;
}
.tot1 {
  font-size: 30rpx;
}
.garden-box {
  display: flex;
  align-items: center;
  margin: 14rpx 0;
}
.types-i {
  margin: 18rpx 0;
  font-size: 28rpx;
  width: 135rpx;
}
.types-input {
  flex: 1;
  padding: 10rpx 0 10rpx 20rpx;
  z-index: 0;
  font-size: 28rpx;
}
.tips input {
  padding: 10rpx 0 10rpx 10rpx;
  background: #f2f2f263;
  border-radius: 6px;
  font-size: 28rpx;
}
.add-pop-btn {
  padding: 20rpx 30rpx 0;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

.add-pop-btn>.add-footer-btn {
  width: 360rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #fff;
  height: 66rpx;
  border-radius: 20px;
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
}
.garden-box>button{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #000;
  margin: 0;
  margin-left: 10rpx;
  padding: 0 30rpx;
  background: #f2f2f2;
  border-radius: 20px;
}