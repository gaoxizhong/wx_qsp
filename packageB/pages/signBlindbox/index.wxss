.signBlindbox-box{
  width: 100%;
  height: 980rpx;
  padding-top: 210rpx;
  background-image: url('https://oss.qingshanpai.com/sign/sign_mhbg.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  box-sizing: border-box;
}
.signBlindbox-dmh{
  width: 352rpx;
  height: 644rpx;
  display: inline-block;
  margin-left: 50%;
  transform: translateX(-50%);
}
.signBlindbox-btn-box{
  width: 548rpx;
  height: 118rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  margin-top: 104rpx;
  position: relative;
  box-sizing: border-box;
  line-height: 118rpx;
  text-align: center;
  color: #ff0000;
  font-weight: bold;
  font-size: 38rpx;
}
.signBlindbox-btn{
  width: 100%;
  height: 118rpx;
  display: inline-block;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.signBlindbox-title{
  width: 100%;
  padding: 6rpx 0;
  text-align: center;
  font-size: 38rpx;
  color: #fff;
  font-weight: bold;
  margin-top: 20rpx;
}
.signBlindbox-advertise-box{
  width: 100%;
  height: auto;
  background: #de0219;
  padding: 20rpx;
  box-sizing: border-box;
}
.swiper-container1 {
  width: 100%;
  height: 406rpx;
  z-index: -1;
  box-sizing: border-box;
  border-radius: 6px;
  box-shadow: 0 1px 10px #00000020;
}
.swiper-container1 swiper {
  height: 100%;
}
.swiperitem-box {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.swiperitem-l {
  width: 430rpx;
  height: 100%;
}
.swiperitem-r {
  flex: 1;
  height: 100%;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}
.swiperitem-l image {
  width: 100%;
  height: 100%;
  border-radius: 8px;
}
.swiperitem-r-title {
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.swiperitem-r-y {
  font-size: 28rpx;
  color: #b8b8b8;
  padding: 10rpx 0;
}
.discount-price {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  margin-top: 12rpx;
  color: #ffbd73;
}
.qg-box {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  text-align: right;
  font-size: 32rpx;
  font-weight: 600;
  color: #ffbd73;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}

.openBlindbox-box{
  width: 100%;
  height: auto;
  padding:20rpx 30rpx;
  box-sizing: border-box;
  background: #feecd3;
}

.is-blindBox {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.is-blindBox>view {
  flex: 1;
  font-size: 30rpx;
  text-align: center;
}
.is-blindBox-jg {
  font-weight: 600;
  color: #de0219;
}
.is-blindBox-jg>text{
  font-size: 54rpx;
  padding-right: 10rpx;
}
.is-blindBox-info {
  width: 100%;
  box-sizing: border-box;
  font-size: 30rpx;
  font-weight: 600;
  color: #de0219;
  text-align: center;
  margin-top: 12rpx;
}
.djs-box{
  text-align: right;
  font-size: 32rpx;
  font-weight: 600;
  color: #e9001c;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.is_progress_bar {
  padding: 20rpx 0;
  box-sizing: border-box;
  margin: 0 auto;
}
.progress-box{
  position: relative;
  box-sizing: border-box;
}
.progress-box progress {
  border-radius: 20px;
}
.progress_btn_box {
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  padding: 10rpx 0;
}

/* ======  商品详情样式  开始========== */
.swiper-container {
  width: 100%;
  height: 420rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.swiper-container .swiper {
  height: 420rpx;
}
.swiper-container image {
  width: 100%;
  height: 420rpx;
}
.price-info {
  padding: 16rpx 20rpx;
  position: relative;
  box-sizing: border-box;
  background: #fff;
}
.price-info .origin-price {
  font-size: 24rpx;
  color: #b8b8b8;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.price-info .info-title {
  width: 90%;
  margin-top: 10rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #000;
  height: auto;
  overflow: hidden;
  box-sizing: border-box;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  align-content: center;
}
.price-info .now-price {
  display: flex;
  align-items: center;
  padding-right: 30rpx;
}
.noe-price-stock {
  padding-left: 20rpx;
  font-size: 26rpx;
  color: #acacac;
  display: flex;
  align-items: center;
  width: auto;
  margin-top: 20rpx;
}
.introduction {
  height: 80rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 36rpx;
  background: #fff;
}
.introduction image {
  width: 32rpx;
  height: 32rpx;
  text-align: center;
}
.introduction text {
  font-size: 26rpx;
  flex: 1;
  margin-left: 26rpx;
  color: #040404;
}
.divide {
  background-color: #F7F7F7;
}
.good-desc {
  margin-bottom: 98rpx;
  padding: 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.desc-imageList {
  padding: 0 10rpx;
  box-sizing: border-box;
  width: 100%;
}
.desc-imageList>image {
  margin: 20rpx 0;
  width: 100%;
}

