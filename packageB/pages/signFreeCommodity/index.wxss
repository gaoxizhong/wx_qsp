.signfree-bg-box{
  width: 100%;
  height: 180rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.signfree-bg-box>image{
  display: inline-block;
  width: 100%;
  height: 100%;
}
.signfreeCommodity-title-box{
  width: 100%;
  height: auto;
  display: flex;
  align-items: flex-end;
  margin-top: -80rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.freeCommodity-title-imagebox{
  width: 250rpx;
  height: 250rpx;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: hidden;
  position: relative;
}
.freeCommodity-title-imagebox>image{
  width: 100%;
  height: 100%;
  display: inline-block;
}
.freeCommodity-info-imagebox{
  padding-left: 20rpx;
  box-sizing: border-box;
  flex: 1;
}
.titleinfo-title{
  font-size: 30rpx;
  font-weight: bold;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.titleinfo-jiage{
  font-weight: bold;
  font-size: 30rpx;
  color: #fa522b;
}
.titleinfo-yuanjia{
  color: #a0a0a0;
  font-size: 26rpx;
  text-decoration: line-through;
}
.chooseWay-box{
  padding: 20rpx 26rpx;
  box-sizing: border-box;
  position: relative;
}
.chooseWay-box::before{
  width: 100%;
  height: 1px;
  content: '';
  background: #dcdcdc;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  bottom: 0;
}
.chooseWay-title-box{
  width: 100%;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.chooseWay-title-icon{
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background: #80cb11;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 10rpx;
}
.chooseWay-title-icon>view{
  background: #fff;
  border-radius: 50%;
  width: 100%;
  height: 100%;
}
.chooseWay-btn-box{
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  margin: 20rpx 0;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.chooseWay-btn-box>view{
  width: auto;
  padding: 16rpx 10rpx;
  border: 1rpx solid #c3c3c3;
  box-sizing: border-box;
  border-radius: 20px;
  color: #696969;
  font-size: 28rpx;
  position: relative;
}
.chooseWay-btn-box>view.choose-icon{
  color: #80cb11;
  border: 1px solid #80cb11;
}
.title-num-imagebox{
  width: 56rpx;
  height: 32rpx;
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
}
.title-num-imagebox>image{
  width: 100%;
  height: 100%;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 0;
}
.title-num-imagebox1{
  width: 100%;
  height: 100%;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 22rpx;
  color: #fff;
}
.member-share-box{
  width: 100%;
