page {
  font-family: PingFangSC-Regular,PingFangSC;
  color: #333;
  background: #ffffff;
  box-sizing: border-box;
}
.bac-box {
  width: 100%;
  height: 290rpx;
  box-sizing: border-box;
  background: #fff;
}


.dl-bothinfo-box {
  width: 100%;
  height: 248rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  padding: 0 120rpx;
  background-image: url(https://oss.qingshanpai.com/icon/adperm-icon-sqbg.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.dl-itemsinfo-box {
  width: 146rpx;
  height: 146rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  border-radius: 50%;
  overflow: hidden;
}
.bothinfo-z-box {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-top: 30rpx;
}
.dl-itemsinfo-box>image {
  width: 146rpx;
  height: 146rpx;
  border-radius: 50%;
}
.dl-itemsinfo-box>.dl-text {
  font-size: 28rpx;
  font-weight: 600;
  margin-top: 10rpx;
}
.bothinfo-z-img {
  width: 58rpx;
  height: 58rpx;
  margin: 4rpx 0;
}

.jfdatainfo-box {
  width: 100%;
  height: 42rpx;
  box-sizing: border-box;
}

.infoPlate-box {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 40rpx 0;
}
.footer-btn-box {
  width: 482rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  border-radius: 8px;
  background-image: linear-gradient(to right, #ffad3c , #ff7d18);
}

.list-title-box{
  padding: 20rpx;
  font-size: 30rpx;
  color: #a0a0a0;
}
.v-pics {
  padding: 0 40rpx;
  font-size: 32rpx;
}
.mine_nav>view {
  background: #fff;
  margin-top: 15rpx;
  padding: 0 40rpx;
}
.mine_nav>view .items {
  color: #000;
  margin: 0;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-radius: 0;
  border-bottom: 1px solid #f4f4f4;
}
.mine_nav>view .items text {
  font-size: 32rpx;
  color: #1a1a1a;
  width: 160rpx;
  text-align: left;
  padding-left: 16rpx;
}
.mine_nav>view .items input {
  text-align: left;
  font-size: 30rpx;
  color: #000;
  flex: 1;
}
.v-pics >view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1px solid #f4f4f4;
}
.v-pics >view >span {
  width: 200rpx;
}
.v-pics .photo {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  border: 1px solid #b5b5b5;
  border-radius: 10rpx;
}
.v-pics .photo image {
  width: 100%;
  height: 100%;
}
.v-pics .photo button {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.v-pics .photo button image {
  position: absolute;
  width: 60%;
  height: 60%;
  top: 20%;
  left: 20%;
}
.shengshi {
  font-size: 30rpx;
}
.itemsImg{
  width: 64rpx;
}