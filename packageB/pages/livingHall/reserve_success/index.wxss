page{
  background: #f4f4f4;
}
.success-titlebox{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  background: #fff;
  font-size: 30rpx;
  padding: 40rpx 30rpx;
}
.success-title{
  width: 100%;
  display: flex;
  align-items: center;
}
.success-text-box{
  flex: 1;
  padding: 10rpx 0 10rpx 10rpx;
  box-sizing: border-box;
}
.success-title-img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}
.success-text-t{
  font-size: 32rpx;
}
.success-text-b{
  font-size: 28rpx;
  color: #999999;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.success-btn{
  width: 100%;
  height: 78rpx;
  background: #f1500a;
  color: #fff;
  font-size: 30rpx;
  border-radius: 6px;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 15px;
}
.userList-box{
  width: auto;
  margin: 0 10px 10px;
}
.zwzpzs-box{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #999;
}
.scroll-view-item_H image {
  width: 100%;
  height: 100%;
}