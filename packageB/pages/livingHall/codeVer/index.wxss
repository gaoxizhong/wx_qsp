page{
  box-sizing: border-box;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.pagecode-1 {
  padding: 30rpx;
  box-sizing: border-box;
  background: #fff;
}
.pagecode-1-box{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.pagecode-1-box>view{
  width: auto;
  padding-right: 14rpx;
}
.pagecode-1-box .input-num {
  display: flex;
  flex: 1;
  height: 72rpx;
  border-radius: 6px;
  background-color: #f5f5f5;
  margin-top: 0;
  font-size: 30rpx;
  color: #000;
  font-family: PingFang-SC-Regular;
  margin-left: 0;
  border: none;
  padding-left: 20rpx;
}
.pagecode-1 .t-b {
  margin-top: 50rpx;
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
  border-radius: 20px;
  height: 70rpx;
  line-height: 70rpx;
  width: 380rpx;
  font-size: 30rpx;
  color: #fff;
}

.page-list-box{
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  background: #f4f4f4;
}
.beusedList-box{
  width: 100%;
}
.beused-items-box{
  width: 100%;
  margin-top: 20rpx;
  background: #fff;
  border-radius: 6px;
  padding: 24rpx 30rpx 30rpx;
  font-size: 30rpx;
  box-sizing: border-box;
}
.beused-items-tbox{
  width: 100%;
  padding-bottom: 24rpx;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.photo-title-box{
  flex: 1;
  display: flex;
  align-items: center;
}
.photo-title-box>.photo-image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  overflow: hidden;
}
.photo-title-box>text{
  font-size: 28rpx;
  padding-left: 10rpx;
}
.beused-items-infobox{
  width: 100%;
  padding: 20rpx 0;
  display: flex;
}
.beused-items-infobox>image{
  width: 105rpx;
  height: 105rpx;
}
.items-infobox-left{
  padding-left: 20rpx;
  font-size: 28rpx;
  color: #565656;
}
.items-btnbox{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.items-btnbox1{
  width: 140rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 28rpx;
  background: #ff0000;
  color: #fff;
  border-radius: 20px;
}
.hoverClass{
  opacity: 0.8;
}
.num {
  width: 220rpx;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  margin-right: 10rpx;
}

.num .input {
  text-align: center;
}
.minuadd-box {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 44rpx;
  font-weight: 600;
  color: #464646;
  padding: 4px 20rpx;
  box-sizing: border-box;
}

.cover {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #00000080;
  overflow: hidden;
  z-index: 999;
}
.toast-box {
  width: 84%;
  height: auto;
  background-color: white;
  opacity: 1;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  border: 1rpx solid white;
  border-radius: 6px;
  padding: 15rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
}
.clickQr{
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
  border-radius: 20px;
  height: 60rpx;
  line-height: 60rpx;
  width: 244rpx;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  margin: 0 auto;
  margin-top: 35rpx;
}
.items-name{
  width: auto;
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 0;
  box-sizing: border-box;
}


.is_tcover {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #00000080;
  overflow: hidden;
  z-index: 999;
}
.is_ttoast-box{
  width: 84%;
