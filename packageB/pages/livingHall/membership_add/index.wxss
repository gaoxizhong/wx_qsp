page{
  padding-top: 30rpx;
  box-sizing: border-box;
}
.pages-title1{
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #000000;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.pages-title{
  font-size: 40rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
  margin-top: 10rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.mine_nav,.v-pics{
  padding: 0 30rpx;
  box-sizing: border-box;
}
.mine_nav{
  margin-top: 60rpx;
}
.v-pics{
  margin-top: 10rpx;
}
.mine_nav>.items {
  color: #000;
  margin: 0;
  display: flex;
  flex-direction: column;
  padding: 20rpx 0;
  border-radius: 0;
  margin-top: 20rpx;
  position: relative;
}
.mine_nav>.items::before{
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  background: #dadada;
  position: absolute;
  left: 0;
  bottom: 0;
}

.mine_nav>view.items text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333;
}
.mine_nav>view.items input,.mine_nav>view.items picker {
  text-align: left;
  font-size: 30rpx;
  color: #000;
  padding-left: 10rpx;
  height: 50rpx;
  margin-top: 20rpx;
}

.v-pics >view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 20rpx 0;
  position: relative;
}
.v-pics >view.v-back::before{
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  background: #dadada;
  position: absolute;
  left: 0;
  bottom: 0;
}
.v-pics >view >span {
  width: auto;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333;
}
.v-pics .photo {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  border: 1px solid #b5b5b5;
  border-radius: 10rpx;
  margin-left: 30rpx;
}
.v-pics .photo image {
  width: 100%;
  height: 100%;
}
.v-pics .photo button {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.v-pics .photo button image {
  position: absolute;
  width: 60%;
  height: 60%;
  top: 20%;
  left: 20%;
}
.xiu-text {
  font-size: 24rpx;
  color: #999;
  padding-left: 60rpx;
}
.send-apply{
  width: 648rpx;
  background: #F2E3C0;
  border-radius: 10px;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #010101;
}
.hover_color{
  opacity: 0.8;
}
.btn-box{
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 0;
  left: 0;
}