.no-branch-box{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
  padding-top: 60px;
  box-sizing: border-box;
}
.no-branch-imagebox{
  text-align: center;
}
.no-branch-imagebox>image{
  width: 224rpx;
}
.no-branch-text{
  padding-top: 20rpx;
  font-weight: 600;
}
.no-branch-btn{
  padding: 6px 24px;
  border: 1px solid #f8510e;
  color: #f8510e;
  border-radius: 20px;
  margin-top: 20px;
  line-height: 1.2;
  font-size: 30rpx;
}
.yes-branch-box{
  width: 100%;
  height: auto;
  padding-top: 50rpx;
  font-size: 30rpx;
}
.branchlist-box{
  margin-top: 80rpx;
  padding: 20rpx 40rpx;
  box-sizing: border-box;
}
.yes-branch-title{
  color: #878787;
}
.branchlist-items-box{
  margin: 20rpx 0;
  display: flex;
}
.branchlist-items-box>.branchlist-items-image{
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
}
.branchlist-items-info{
  height: 100%;
}
.applicant_name {
  font-size: 30rpx;
  padding: 0 0 15rpx 30rpx;
}
.approval_state {
  color: #8D8D8D;
  font-size: 28rpx;
  padding-left: 30rpx;
}