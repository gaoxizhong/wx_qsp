page{
  background: #f2f2f2;
  padding: 24rpx;
  box-sizing: border-box;
  font-size: 32rpx;
}
.todayLike-title-box{
  width: 100%;
  height: 226rpx;
  box-sizing: border-box;
  border-radius: 8px;
  overflow: hidden;
  position: relative;
}
.todayLike-title-bg{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.todayLike-title-info{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  padding: 0 50rpx;
  box-sizing: border-box;
}
.title-info-top{
  flex: 1;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: flex-end;
  padding-bottom: 10rpx;
}
.titleInfo-ljdh-box{
  width: 146rpx;
  height: 76rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  font-weight: bold;
  color: #fff;
  text-align: center;
  line-height: 1.5;
  position: relative;
  padding-top: 10rpx;
}
.titleInfo-ljdh-box>image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.titleInfo-title{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  height: 100%;
  padding-top: 24rpx;
  box-sizing: border-box;
  color: #b62d1a;
  padding-bottom: 16rpx;
}
.titleInfo-title-t{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.titleInfo-title-t>image{
  width: 34rpx;
  margin-left: 2px;
}
.todayLike-bottom-ye{
  width: 100%;
  height: 78rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #af1812;
}
.titleInfo-num{
  font-size: 38rpx;
  font-weight: bold;
  color: #de0200;
}
.bgmode{
  width: 10rpx;
  margin-left: 10rpx;
}
/* ==================================== 头部账户信息以上 ================================= */
.todayLike-listitems-box{
  width: 100%;
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 8px;
  overflow: hidden;
  margin: 20rpx 0;
}
.listitems-top-box{
  width: 100%;
  display: flex;
}
.toplw-listitems-box{
  flex: 1;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 16rpx;
}
.add-icon-box{
  width: 146rpx;
  display: flex;
  font-size: 28rpx;
  justify-content: flex-end;
}
.add-icon{
  width: 20rpx;
}
.add-icon-box>text{
  font-size: 26rpx;
  padding-left: 8rpx;
}
.listitems-lwlist {
  width: auto;
  position: relative;
  padding-right: 14rpx;
}
.dzlw-listitems-lwimg {
  width: 94rpx;
}
.lw-num {
  color: #fb2d56;
  font-size: 30rpx;
