  align-items: center;
  color: #a9a9a9;
}
.cxbtn-r-box{
  width: auto;
  height: 76rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.listitems-cxbtn-hh{
    width: auto;
