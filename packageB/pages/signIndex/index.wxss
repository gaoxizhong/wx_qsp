.sigin-dt-box{
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 8px;
}
.siginDt-title-box{
  display: flex;
  align-items: center;
  box-sizing: border-box;
}
.siginDt-title-box>image{
  width: 52rpx;
}
.siginDt-text{
  width: 100%;
