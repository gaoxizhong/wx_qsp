}
.circle-img {
  margin-top: 10rpx;
  margin-bottom: 18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
}
.imgyi-width {
  width: 380rpx;
  margin-top: 10rpx;
  vertical-align: top;
}
.circle-footer-operation{
  padding: 10rpx !important;
  margin-top: 46rpx;
  height: 44rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  position: relative;
}
.circle-footer-operation .item-btn {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: auto;
  line-height: 1.5;
  font-size: 30rpx;
  position: relative;
  padding: 10rpx 0;
  overflow: visible;
}
.circle-footer-operation .item-btn:nth-of-type(1){
  justify-content: flex-start;
}
.circle-footer-operation .item-btn:nth-of-type(3){
  justify-content: flex-end;
}
.circle-footer-operation .item-btn text.text {
  font-size: 30rpx;
  color: #979797;
  padding-left: 10rpx;
}
.circle-footer-operation .index-ic-sub {
  position: absolute;
  width: 82rpx;
  height: 46rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  box-sizing: border-box;
}

.index-ic-sub image {
  width: 82rpx;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.index-ic-sub text {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  display: inline-block;
  width: 100%;
  padding-top: 2rpx;
}
