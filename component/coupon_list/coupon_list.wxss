.coupon-list-box {
  box-sizing: border-box;
  width: 100%;
  padding: 0 30rpx;
  font-size: 28rpx;
}
.coupon-list-items:nth-of-type(3n-2){
  background-image: url(data:image/png;base64,);
}
.coupon-list-items:nth-of-type(3n-1){
  background-image: url(data:image/png;base64,);
}
.coupon-list-items:nth-of-type(3n){
  background-image: url(data:image/png;base64,);
} 

.coupon-list-items{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 0;
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  border-radius: 10px;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin: 24rpx 0;
  position: relative;
  box-shadow: 0 0 20rpx 2rpx #00000020;
}

.coupon-list-items.select-items::after{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  border: 3px solid #00b377;
  border-radius: 10px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  -webkit-transform-origin: left top;
  transform-origin: left top;
  z-index: 999;
}
.sheng-bg{
  width: 50rpx;
  height: 46rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 99;
}
.sheng-bg image{
  width: 100%;
  height: 100%;
}
.listitems-le{
  padding: 0 6rpx;
  width: 206rpx;
  height: 100%;
  box-sizing: border-box;
  position: relative;
}
.listitems-le-title,.calendar-title,.calendar-con,.shops-name,.listitems-ri .coupon-title,.listitems-ri .ri-price-curr,.listitems-ri .coupon-view,.listitems-ri .start-end-time{
  overflow: hidden;
  text-overflow: ellipsis; /*超出用省略号显示 */
  display:-webkit-box; /*作为弹性伸缩盒子模型显示。*/
  -webkit-box-orient:vertical; /*设置伸缩盒子的子元素排列方式--从上到下垂直排列*/
  -webkit-line-clamp:1; /*显示的行*/
}
.listitems-le-title{
  font-size: 24rpx;
  color: #fff;
  text-align: left;
}
.calendar-box{
  width: 174rpx;
  height: 80rpx;
  box-sizing: border-box;
  position: absolute;
  left: 50%;
  top: 60rpx;
  transform: translateX(-50%);
}
.calendar-con {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 42rpx;
  color: #fff; 
}
.shops-name {
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  bottom: 28rpx;
  transform: translateX(-50%);
  width: 100%;
  padding: 0 14rpx;
  box-sizing: border-box;
}
.listitems-ri{
  flex: 1;
  box-sizing: border-box;
  height: 100%;
  position: relative;
  overflow: hidden;
  padding: 16rpx 80rpx 16rpx 14rpx;
}
.listitems-ri>view{
  box-sizing: border-box;
}
.listitems-rititle-box{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.listitems-rititle-box .coupon-title{
  color: #000;
}
.coupon-title-stock{
  width: auto;
  font-size: 26rpx;
  position: absolute;
  top: 10rpx;
  right: 40rpx;
}
.ri-price-curr,.coupon-view{
  font-size: 24rpx;
  margin-top: 10rpx;
}
.start-end-time{
  position: absolute;
  bottom: 4rpx;
  left: 14rpx;
  font-size: 22rpx;
  color: #c9cbcf;
}
.use-btn{
  width: auto;
  height: auto;
  position: absolute;
  right: 20rpx;
  box-sizing: border-box;
}
.use-btnstock{
  width: 62rpx;
  height: 62rpx;
}
.use-btnstock>image{
  width: 100%;
  height: 100%;
}
.use-btnstock_else{
  width: 146rpx;
  height: 146rpx;
}
.use-btnstock_else>image{
  width: 100%;
  height: 100%;
}
.stock_a{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}