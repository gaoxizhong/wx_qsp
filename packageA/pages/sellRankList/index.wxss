page {
  font-family: PingFangSC-Regular,PingFangSC;
  color: #333;
  background: #f2f2f2;
  padding-top: 30rpx;
}
.integral-transaction {
  padding: 0;
  box-sizing: border-box;
  margin: 0 30rpx;
  border-radius: 8px;
  background: #fff;
  overflow: hidden;
  padding-bottom: 50rpx;
}
.sellRank-img {
  width: 100%;
  height: 168rpx;
  position: relative;
  overflow: hidden;
}
.sellRank-img image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.sellRank-img-view {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  font-weight: 600;
  color: #fff;
  box-sizing: border-box;
  text-align: center;
  padding-top: 100rpx;
  padding-right: 20rpx;
}
.sellRank-module-box {
  width: 100%;
  padding: 10rpx 20rpx 30rpx;
  box-sizing: border-box;
  position: relative;
}
.sellRankList-box {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.sellRank-citveBox {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
}
.sellRank-citveBox>view.sellRank-citveBox-items {
  flex: 1;
  height: auto;
  border-radius: 6px;
  text-align: center;
  position: relative;
  overflow: hidden;
  padding: 10rpx 0;
}
.sellRank-citveBox-items .sellRank-citveBox-subtitle {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #949494;
  box-sizing: border-box;
}
.sellRank-citveBox-subtitle>image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
}
.sellRank-citveBox-subtitle>text {
  font-size: 24rpx;
  padding-left: 10rpx;
}
.sellRank-citveBox-items.itemsHover::after{
  content: '';
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  border: 1px solid #000;
  box-sizing: border-box;
  width: 20%;
  z-index: 1;
}
.ranklist-items.ranklist-items1 {
  border-bottom: 1px solid #999;
  margin-bottom: 30rpx;
}
.ranklist-items {
  width: 100%;
  padding: 14rpx 0;
  position: relative;
  display: flex;
  align-items: center;
  box-sizing: border-box;
}
.ranklist-items.ranklist-items2 {
  padding: 20rpx 0;
}
.ranklist-items>view {
  font-size: 28rpx;
}
.ranklist-items>view:nth-of-type(1) {
  width: 50rpx;
  height: 50rpx;
  padding: 0 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
}
.ranklist-items>view.rank-avatar {
  flex: 1;
  padding: 0 12rpx;
  display: flex;
  align-items: center;
}
.ranklist-items>view.rank-money-box {
  width: 164rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.ranklist-items>view.rank-avatar image {
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
}
.ranklist-items>view.rank-index image {
  width: 50rpx;
  height: 50rpx;
}

