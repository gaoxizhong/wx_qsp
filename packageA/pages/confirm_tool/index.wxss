page {
  background: #fff;
  box-sizing: border-box;
}
.coupon-list-items {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 0;
  position: absolute;
  left: 30rpx;
  top: 0;
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  border-radius: 10px;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  box-shadow: 0 0 20rpx 2rpx #00000020;
}
.product-title {
  padding: 30rpx;
}
.prduct-box {
  width: 100%;
  height: auto;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.liuyan-box {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  display: flex;
  align-items: flex-start;
  padding: 30rpx;
}
.prduct-img-box {
  width: 100%;
  height: 100%;
  padding: 30rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 8px 2px #00000015;
  position: relative;
}
.prduct-img-box>image {
  height: 360rpx;
}
.liuyan-box-left {
  width: 84rpx;
  height: 84rpx;
  border-radius: 6px;
  overflow: hidden;
}
.liuyan-box-right {
  flex: 1;
  padding-left: 20rpx;
}
.liuyan-box-left>image {
  width: 100%;
  height: 100%;
}
.dianzhang-info {
  padding: 16rpx 20rpx;
  background: #89d563;
  color: #fff;
  margin-top: 10rpx;
  position: relative;
  font-size: 26rpx;
  border-radius: 3px;
}
.dianzhang-info>image{
  width: 8rpx;
  height: 16rpx;
  position: absolute;
  left: -8rpx;
  top: 4rpx;
}
.bottom-box1 {
  width: 100%;
  height: 140rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  box-shadow: 0 0 10rpx 0 #00000015;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 1;
}
.bottom-box1>view{
  width: 200rpx;
  height: 76rpx;
  overflow: hidden;
  position: relative;
}
.bottom-box1>view.bottom-box-ri{
  width: 370rpx;
}
.bottom-box-ri.you-box-ri{
  border-radius: 20px;
  color: #fff;
  background: #ffbd73;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.bottom-box-ri.shang-box-ri{
  width: auto;
  border-radius: 20px;
  color: #fff;
  background: #f73b3b;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.bottom-box1>view>image {
  width: 100%;
  height: 100%;
}
.page_title{
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  color: #303030;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.page_title_view{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.jia-jifen{
  padding: 12rpx 36rpx;
  width: auto;
  height: auto;
  color: #000;
  font-size: 30rpx;
  border-radius: 6px;
  position: fixed;
  right: 64rpx;
  bottom: 60rpx;
  opacity: 0;
  background: #fff;
  box-shadow: 0 0 10rpx 2rpx #00000020;
}
.jia-jifen2{
  padding: 12rpx 36rpx;
  width: auto;
  height: auto;
  color: #000;
  font-size: 30rpx;
  border-radius: 6px;
  position: fixed;
  left: 64rpx;
  bottom: 60rpx;
  opacity: 0;
  background: #fff;
  box-shadow: 0 0 10rpx 2rpx #00000020;
}
.commodity-items {
  position: relative;
  height: auto;
  padding: 20rpx 30rpx;
}
.circle {
  background: #fff;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.shop-centent {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.shop-centent-title {
  height: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: stretch;
}
.shop-img {
  width: 144rpx;
  height: 144rpx;
  overflow: hidden;
}
.shop-img>image{
  width: 100%;
  height: 100%;
}
.shop-jottings {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  padding-left: 20rpx;
  overflow: hidden;
}
.shop-jottings .text, .shop-jottings .text.cool {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.circle-head {
  display: flex;
  align-items: center;
  position: relative;
}
.discount {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  margin-top: 12rpx;
}
.discount-price {
  color: #D55B06;
}

  