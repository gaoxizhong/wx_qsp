.item-box{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 16rpx 26rpx; 
  border-bottom: 1px solid #dddddd;
  position: relative;
}
.item-box .item-left-icon{
  width: 48rpx;
  height: 72rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.item-box .item-right{
  color: #dfdfdf;
}
.biaozhun{
  font-size:28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 6rpx 36rpx 6rpx 86rpx;
  color: #ffae00;
}
.min_icon {
  display: flex;
  align-items: center;
  margin-left: auto;
  margin-right: 20rpx;
}
.min_icon image {
  width: 12rpx;
  margin-left: 40rpx;
}








.footer-btn-box{
  width: 100%;
  height: auto;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 20rpx 38rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  box-shadow: 1rpx 1rpx 6rpx 2rpx #dfdfdf;
  background: #fff;
}
.footer-btn-box>view.footer-box-text{
  width: 50%;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  color: #9e9e9e;
  font-size: 30rpx;
}
.footer-btn-box>view.footer-btn{
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #ff6b00;
  color: #fff;
  height: 68rpx;
  font-size: 30rpx;
}