page{
  background: #f2f2f2;
  padding: 20rpx 25rpx;
  box-sizing: border-box;
}
.createMerchantBuy-instruction-box{
  background: #fff;
  width: 100%;
  font-size: 30rpx;
  border-radius: 8px;
  min-height: 164rpx;
  overflow: hidden;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 30rpx 20rpx 0;
}
.announcement-info-left {
  width: auto;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
  align-items: flex-start;
}
.announcement-info-left>image {
  width: 42rpx;
  height: 42rpx;
}
.announcement-info {
  flex: 1;
  height: 164rpx;
  box-sizing: border-box;
  overflow: hidden;
  padding-left: 20rpx;
}
.createMerchantBuy-ChooseAmount-box{
  background: #fff;
  width: 100%;
  font-size: 30rpx;
  border-radius: 8px;
  padding: 0 0 20rpx 0;
  box-sizing: border-box;
  margin: 20rpx 0;
}
.chooseAmount-title-box{
  width: 100%;
  display: flex;
  align-items: center;
  padding:20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.chooseAmount-title-img{
  width: 34rpx;
  height: 34rpx;
}
.chooseAmount-title-text{
  padding-left: 20rpx;
  font-weight: 600;
}
.chooseAmount-items-box{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  box-sizing: border-box;
}
.chooseAmount-items-box>view{
  width: 50%;
  box-sizing: border-box;
  padding: 10rpx;
}
.chooseAmount-items-box>view .chooseIemsList{
  box-sizing: border-box;
  border-radius: 6px;
  border: 1px solid #dcdcdc;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  height: 166rpx;
}
.chooseIemsList .chooseIemsList-input{
  text-align: center;
}
.chooseAmount-items-box>view .chooseIemsList.selectMoney{
  border: 1px solid #ff9005;
  color: #ff9005;
}
.chooseIemsList-img{
  width: 48rpx;
  height: 48rpx;
}
.chooseIemsList>view{
  flex: 1;
  font-size: 28rpx;
}
.module-box{
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  border-radius: 8px;
  padding: 20rpx 0;
}
.select-box {
  width: auto;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
}
.select-box image {
  width: 106rpx;
  height: 106rpx;
}
.chooseDays-box{
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  border-radius: 8px;
  margin: 20rpx 0;
}
.daysItems-box{
  width: 100%;
  padding: 20rpx 10rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.daysItems-box>view{
  width: auto;
  border-radius: 20px;
  border: 1px solid #dcdcdc;
  margin: 10rpx;
  padding: 6rpx 30rpx;
  position: relative;
}
.select-time.sele-hover{
  border: 1px solid #76cf00;
  color: #76cf00;
}
.select-time-img{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}
.location-box, .kilometerItems-box {
  padding: 20rpx 0;
  box-sizing: border-box;
  width: 100%;
}
.kilometerItems-box1 {
  padding-top: 30rpx;
}
.location-box1, .kilometerItems-box1 {
  width: 100%;
  height: auto;
  background: #fff;
  border-radius: 8px;
  box-sizing: border-box;
}
.location-nav-box {
  height: 84rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  margin: 0 20rpx;
  position: relative;
}
.location-nav-box::before {
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  position: absolute;
  bottom: 0;
  left: 0;
  background: #dedede;
}
.location-nav-box view.nav-items-box {
  height: 100%;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #aaaaaa;
  font-size: 28rpx;
  position: relative;
}
.nav-items-box.after::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  border: 1.5px solid #000;
  box-sizing: border-box;
  width: 58rpx;
  z-index: 1;
  background-color: #000;
}
.nav-items-box>image {
  height: 32rpx;
}
.nav-items-box>text {
  padding-left: 4px;
}
.address-box {
  width: 100%;
  display: flex;
  align-items: flex-start;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 20rpx 30rpx 0 30rpx;
}
.address-title {
  width: auto;
  color: #000;
}
.address-text {
  flex: 1;
  color: #999;
  padding-left: 20rpx;
}
.filter-box {
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 20px;
  margin: 0 20rpx;
  margin-top: 30rpx;
  background: #f8f8f8;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.filter-box>.filter-image-box {
  width: 48rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.filter-box-c {
  flex: 1;
  font-size: 30rpx;
  padding-left: 20rpx;
}
.filter-box>.filter-image-box {
  width: 48rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.image-box-l {
  width: 32rpx;
}
.image-box-r {
  width: 34rpx;
}
.infoPlate-box {
  margin: 0 30rpx;
  display: flex;
  flex-direction: column;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.infoPlate-Push-box {
  display: flex;
  align-items: center;
  justify-content: center;
}
.infoPlate-Push-box>image {
  width: 336rpx;
}
.hoverClass {
  color: #000;
}
.list-box-items {
  flex: 1;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 30rpx;
}
.distance-item {
  width: 140rpx;
  height: 60rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  text-align: center;
  background: #fff;
  border-radius: 20px;
  margin-left: 24rpx;
  margin-bottom: 24rpx;
  position: relative;
  border: 1rpx solid #dfdfdf;
}
.distance-item.highlight-item {
  border: 1rpx solid #72cf00;
  color: #72cf00;
}
.sel-image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.distance-btn-box {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 20rpx 0;
}
.dist-btn {
  width: 320rpx;
  height: 76rpx;
  line-height: 75rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: 600;
  color: #fff;
  border-radius: 20px;
  background-image: linear-gradient(to right, #ff6d00, #ffa300);
}

/* /////////////////  选择展示的商品/优惠券   //////////////////////////// */
.circle {
  padding: 14rpx 40rpx;
  background: #fff;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  border-radius: 6px;
}
.shop-jottings .text, .shop-jottings .text.cool {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.shop-centent {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.shop-centent-title {
  height: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: stretch;
}
.shop-img {
  width: 144rpx;
  height: 144rpx;
  overflow: hidden;
}
.shop-jottings {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  padding-left: 20rpx;
  overflow: hidden;
}
.shop-img .shop-img-width {
  width: 100%;
  height: 100%;
}
.shop-jottings .text {
  font-size: 28rpx;
  height: auto;
  overflow: hidden;
}
.circle-head {
  display: flex;
  align-items: center;
  position: relative;
}
.discount {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  margin-top: 12rpx;
}
.discount-price {
  color: #D55B06;
}
.again-box {
  width: 100%;
  padding: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-self: center;
  box-sizing: border-box;
}
.again-btn {
  display: inlie-black;
  padding: 0 32rpx;
  height: 54rpx;
  line-height: 54rpx;
  background: #f19149;
  font-size: 26rpx;
  color: #fff;
  border-radius: 20px;
}
.coupon-list-items {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 0;
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  border-radius: 10px;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin: 24rpx auto;
  position: relative;
  box-shadow: 0 0 20rpx 2rpx #00000020;
}
.coupon-list-items{
  background-image: url(data:image/png;base64,);
}
.listitems-ri {
  padding: 16rpx 80rpx 16rpx 14rpx;
}

.hoverclass{
  opacity: 0.8;
}