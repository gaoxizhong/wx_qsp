
/* ================================= */
page {
  box-sizing: border-box;
  font-family: PingFangSC-Regular,PingFangSC;
  background: #f2f2f2;
}
.cates {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx 0 30rpx;
  margin: 10rpx 0;
}
.cates .cateslist {
  height: auto;
  line-height: 50rpx;
  width: 100%;
  white-space: nowrap;
  padding: 10rpx 0;
}
.cates image {
  width: 100%;
  height: 120rpx;
  border-radius: 8rpx;
}
.cates .cateslist .cate-item {
  margin-right: 10rpx;
  display: inline-block;
  font-size: 26rpx;
  background: #76ddb8;
  color: #fff;
  border-radius: 20px;
  padding: 0 22rpx;
  font-weight: 600;
}
.columns {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
}
.columns .columnslist {
  height: 62rpx;
  line-height: 62rpx;
  width: 100%;
  white-space: nowrap;
}
.columns image {
  width: 100%;
  height: 120rpx;
  border-radius: 8rpx;
}
.columns .columnslist .column-item {
  margin-right: 60rpx;
  display: inline-block;
  font-size: 32rpx;
  position: relative;
}
.columns .columnslist .column-item.column-text:after{
  content: "";
  width: 100%;
  height: 4px;
  background: #4ac081;
  position: absolute;
  bottom: 4rpx;
  left: 0;
  opacity: 0.8;
}
.shoplist .shop-item {
  padding: 0 20rpx;
}
.shoplist .shop-item .shop-box {
  font-size: 24rpx;
  padding: 20rpx 4rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  margin: 20rpx 0;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.hbb-items{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 38rpx 14rpx;
  box-sizing: border-box;
}

.hbb-items>view{
  width: 25%;
  height:194rpx; 
  display: flex;
  justify-content: center;
  align-items: center;
  padding:8rpx;
  box-sizing: border-box;
}
.hbb-items>view>.items-list{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: relative;
  box-sizing: border-box;
  padding: 16rpx 10rpx;
}
.hbb-items>view>.items-list>image{
  position: absolute;
  left: 0;
  top: 0;
  max-width: 100%;
  height: 100%;
  z-index:-1;
}
.items-list .hbb-items-title{
  font-size: 26rpx;
  color: #fff;
  text-align: center;
}
.items-list .hbb-items-img{
  width: 56rpx;
  height: 50rpx;
  overflow: hidden;
}
.items-list .hbb-items-img>image{
  width: 56rpx;
  height: 50rpx;
}
.items-list .hbb-items-btn{
  width: 94rpx;
  padding: 4rpx 8rpx;
  font-size: 20rpx;
  background: #fff;
  color: #FF6623;
  text-align: center;
  border-radius: 8px;
}
.inner-layer{
  width:100%;
  height:100%;
  overflow: hidden;
  position: relative;
  border-radius: 6px;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.inner-layer>image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
}
.view-box-iamge image{
  width: 60rpx;
  height:60rpx;
}

.shop-item-1{
  width: 50%;
  padding: 8rpx;
  box-sizing: border-box;

}
.shoplist-1 .shop-item-1 .shop-box-1 {
  font-size: 24rpx;
  display: flex;
  width: 100%;
  height: auto;
  border-radius: 6px;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.shop-item-1 .shop-box-1 .like-image{
  width: 100%;
  height: 300rpx;
  position: relative;
}
.shop-item-1 .shop-box-1 .like-list-info{
  flex:1;
  overflow: hidden;
  background: #fff;
  padding: 0 10rpx;
}
.like-distance{
  height: 40rpx;
  width: auto;
  background: #c8cdd6;
  color: #000;
  font-size: 24rpx;
  padding: 0 16rpx;
  line-height: 40rpx;
  border-bottom-left-radius: 12px;
  border-top-left-radius: 12px;
  overflow: hidden;
  position: absolute;
  top: 16rpx;
  right: 0;
}
.like-image>image{
  width: 100%;
  height: 100%;
}
.like-price{
  margin-top: 6rpx;
}
.like-discount-price{
  font-size:28rpx;
  color:red;
  font-weight:600;
}
.like-price-text{
  color:#979797; 
  font-size:24rpx;
  padding-left:14rpx;
  text-decoration: line-through;
}
.shop-name-box{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:6rpx 8rpx 20rpx 8rpx;
  box-sizing: border-box;

}
.shop-name1{
  flex: 1;
  overflow:hidden; /*超出的文本隐藏*/
  text-overflow:ellipsis; /*溢出用省略号显示*/
  white-space:nowrap; /*溢出不换行*/
  font-size: 30rpx;
}
.shop-name-image{
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  overflow: hidden;
}
.shop-name-image>image{
  width: 100%;
  height: 100%;
}
.befor-after-box{
    padding: 12rpx 0;
    display: flex;
    align-items: center;
    font-size: 24rpx;
}
.radar-scanning{
  width: 50%;
  height: 50%;
  position: absolute;
  left: 0;
  top: 0;
  -webkit-animation:myRotate 10s linear infinite;
  animation:myRotate 10s linear infinite;
  transform-origin: bottom right;
}
@keyframes myRotate{
  0%{ -webkit-transform: rotate(0deg);}
  50%{ -webkit-transform: rotate(180deg);}
  100%{ -webkit-transform: rotate(360deg);}
}


.position-box{
  width: auto;
  height: 32rpx;
  line-height: 32rpx;
  padding: 0 20rpx;
  border-radius: 20px;
  position: absolute;
  top: 42rpx;
  right: -8rpx;
  background: #80d68d;
  color:#fff;
  font-size: 20rpx;
}
.bottom-box{
  width: 100%;
  height: 120rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.bottom-box>view{
  height: 82rpx;
  border-radius: 10px;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bottom-box-lf{
  width: 420rpx;
}
.bottom-box-ri{
  width: 230rpx;
}
.bottom-box-view{
  width: 100%;
  height: 100%;
  border-radius: 6px;
  overflow: hidden;
}
.bottom-box .bottom-box-view>image{
  width: 100%;
  height: 100%;
}
.bottom-box>view>view.number_view{
  display: flex;
  font-size: 22rpx;
  color: #f77a6c;
  position: absolute;
  top: -14rpx;
  right: -2rpx;
  width: 48rpx;
  height: 48rpx;
  background: #f76868;
  color: #fff;
  justify-content: center;
  align-items: center;
  border-radius: 50%;

}
.goodinfo {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx 20rpx;
}
.goodinfo>view.goToInfo {
  width: 50%;
  height: auto;
  margin: 20rpx 0;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.goodinfo>view.goToInfo .good-item-view {
  width: 100%;
  position: relative;
  height: auto;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: hidden;
  box-shadow: 0rpx 0rpx 4px#00000020;
  background: #fff;
}
.good-item-view>view.imagebox{
  width: 100%;
  height: 384rpx;
  position: relative;
}
.good-item-view image.good-item-img {
  width: 100%;
  height: 100%;
  justify-content: center;
  display: block;
  border-radius: 6px;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.good-item-view .goodname {
  font-size: 28rpx;
  color: #333;
  display: block;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  margin-top: 10rpx;
  padding: 0 10rpx;
  font-weight: 600;
}
.member-view{
  width: 100%;
  height: 40rpx;
  position: absolute;
  left: 0;
  bottom: 10px;
  z-index: 2;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.member-view-1{
  background: #00000060;
  border-radius: 20px;
  width: 100%;
  height: 100%;
}
.member-view-2{
  position: absolute;
  left: 10px;
  top: 0;
  border-radius: 20px;
  width: 88%;
  height: 100%;
  padding-left: 10px;
  font-size: 24rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.member-view-2-add{
  flex: 1;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.idle-avar-box {
  width: 100%;
  display: flex;
  align-items: flex-start;
  box-sizing: border-box;
  padding: 10rpx 10rpx;
}
.idle-avar-box>image {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
}
.idle-avar-box>text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #999;
  flex: 1;
}
.sousuo-box {
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  padding: 0 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.input-box {
  height: 62rpx;
  border-radius: 6px;
  overflow: hidden;
  width: 100%;
  margin: 12rpx 0;
  background: #eeeeee;
}
.input-box input {
  height: 100%;
  width: 100%;
  padding-left: 12rpx;
  font-size: 28rpx;
  text-align: center;
  background: #f3f3f3;
}
.btn-box {
  width: 112rpx;
  height: 60rpx;
  font-size: 28rpx;
  background: #fcab53;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 9999;
}
.item-title-name{
  padding:20rpx 0;
  height: 172rpx;
  box-sizing: border-box;
}
.idle-avar-text{
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}