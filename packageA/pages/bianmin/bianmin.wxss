.page-title-box{
  width:85%;
  height:240rpx;
  margin: 0 auto;
}
.page-title-box image{
  width:100%;
  height:100%;
}
.legend{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

}
.legend text{
  font-weight: 600;
  font-size:32rpx;
  color:#6b6e6d;
  padding: 4rpx 10rpx;
}
.norm{
  display: flex;
  align-items: center;
  justify-content: center;
}
.fill-adds-box{
  padding: 0 24rpx;
}
.fill-adds-list{
  margin: 35rpx 0;
}
.adds-list-title{
  margin-left: 38rpx;
  position: relative;
}
.list-title1 {
  content: '';
  width: 18rpx;
  height: 18rpx;
  border-radius: 50%;
  background-color: #29c572;
  position: absolute;
  left: -30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.list-title2 {
  content: '';
  width: 18rpx;
  height: 18rpx;
  border-radius: 50%;
  background-color: #9b0000;
  position: absolute;
  left: -30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.list-title-input{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 8rpx 2rpx 8rpx 20rpx;
}
.list-title-input>view.title1-input1,.list-title-input>view.title2-input1{
 width: 70%;

}
.list-title-input>view.title3-input{
  width: 98%;
  padding-left: 14rpx;
}
.list-title-input>view.title1-input2,.list-title-input>view.title2-input2{
  width: 30%;
}
.list-title-input>view.title1-input1 input,.list-title-input>view.title2-input1 input{
  width: 90%;
  background: #eeeeee;
  line-height: 1;
  font-size: 28rpx;
  padding: 10rpx 16rpx;

 }
 .list-title-input>view.title3-input input{
  width: 90%;
  background: #eeeeee;
  line-height: 1;
  font-size: 28rpx;
  padding: 10rpx 16rpx;

 }
.list-title-input>view.title1-input2 input,.list-title-input>view.title2-input2 input{
  width: 94%;
  background: #eeeeee;
  line-height: 1;
  font-size: 28rpx;
  padding: 10rpx 0;
  text-align: center;
}
.items {
  color: #000;
  margin: 0;
  display: flex;
  padding: 30rpx 0;
  border-radius: 0;
  align-items: center;
  padding-left: 30rpx;
}
.time-picker {
  font-size: 30rpx;
  color: #424242;
  flex: 1;
  padding: 8rpx;

}
.items input {
  text-align: left;
  font-size: 28rpx;
  color: #000;
  flex: 1;
  overflow: auto;
}
.items .min_icon {
  display: flex;
  align-items: center;
  margin-left: auto;
  margin-right: 20rpx;
}
.items .min_icon image {
  width: 12rpx;
  margin-left: 40rpx;
}
.items-adds{
  display: flex;
  align-items: center;
  padding: 30rpx 34rpx 8rpx 20rpx;

}
.items-adds>text {
  padding-right: 12rpx;
  text-align: left;
  color: #1a1a1a;
  font-size: 30rpx;
}
.items-adds .shengshi {
  width: 100%;
  padding: 10rpx 0 10rpx 20rpx;
  font-size: 28rpx;
  color: #333232;
  background: #eeeeee;
}
.submit-btn{
  /* position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%); */
  margin: 0 auto;
  width:88%;
  height:80rpx;
  border-radius:40rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
}
.norm icon.iconfont{
  font-size: 34rpx;
  margin-bottom: 12rpx;

}
.mark{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
  background: #333333;
  opacity: 0.6;
} 
.tanchang{
  padding: 14rpx 28rpx;
  width: 70%;
  height: auto;
  background: #fff;
  font-size: 28rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 99999;
  border-radius: 10px;
}
.tanchang .colse{
  font-size: 30rpx;
  padding: 10rpx 20rpx;
  position: absolute;
  top: 0rpx;
  right: 2rpx;
}
.toll-box{
  padding: 12rpx 0;
}
.toll-box>view{
  padding: 12rpx 0;
  font-size: 28rpx;
}