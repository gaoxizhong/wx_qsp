page{
  background: #f2f2f2;
  padding: 20rpx;
  box-sizing: border-box;
}
.hdrt-box{
  background:#fff;
  box-sizing: border-box;
  padding-bottom: 20rpx;
}
.title1-box{
  font-size: 30rpx;
  padding: 30rpx 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.title1-box image{
  width: 36rpx;
  margin-right: 10rpx;
}
.title1-right-box{
  color: #9e9e9e;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.title1-right-box>image{
  width: 30rpx;
}
.title-box{
  font-weight: 600;
  font-size: 30rpx;
  padding: 30rpx;
}
.cate.lt {
  box-sizing: border-box;
  display: flex;
  flex-wrap: nowrap;
  justify-content: center;
  align-items: center;
  padding: 0 14rpx;
}
.cate-item {
  height: auto;
  color: #666;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.cate.lt>view {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.cate.lt .cate-name1 {
  width: 100rpx;
  height: 100rpx;
  background: #d2d2d2;
  border-radius: 50%;
  position: relative;
}
.cate.lt>view .cate-name {
  font-size: 26rpx;
  text-align: center;
  padding: 10rpx 0;
}
.lt-image {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  bottom: -35rpx;
  left: 50%;
  transform: translateX(-50%);
}
.cate.lt>view image.imagurl {
  width: 100rpx;
  height: 100rpx;
}
.cate.lt .cate-name1 .cate-name1-image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100rpx;
  height: 100rpx;
  opacity: 0.8; 
}
.hd.rt {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
  min-height: 180rpx;
  margin-top: 36rpx;
}
.hd .items {
  display: flex;
  padding: 10rpx 26rpx;
  width: 50%;
  box-sizing: border-box;
}
.items-list {
  font-size: 28rpx;
  height: 70rpx;
  width: 100%;
  border-radius: 6px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #f1f1f1;
}
.items-list.items-list-hove {
  border: 4rpx solid #7fc807;
  color: #7fc807;
  position: relative;
}
.items-img-box{
  width: 42rpx;
  position: absolute;
  right: -14rpx;
  bottom: -14rpx;
}
view.reclaim_title {
  position: relative;
  overflow: hidden;
  line-height: 1;
  margin: 22rpx 36rpx 12rpx 40rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  padding: 0;
}
.reclaim_title text {
  width: 100%;
  font-size: 30rpx;
  font-weight: 700;
  color: #000;
  text-align: left;
  padding-bottom: 4rpx;
}
.tips {
  padding: 0 20rpx;
  position: relative;
}
.tot1 {
  font-size: 30rpx;
}
.name-phone {
  display: flex;
}
.garden-box {
  display: flex;
  align-items: center;
  margin: 14rpx 0;
}
.types-i {
  margin: 18rpx 0;
  font-size: 28rpx;
}
.shengshi {
    font-size: 28rpx;
    color: #333232;
    background: #f2f2f2;
    line-height: 2.5;
    height: 100%;
    margin-left: 20rpx;
    padding-left: 10rpx;
    border-radius: 6px;
}
.types-input {
  flex: 1;
  padding: 10rpx 0 10rpx 20rpx;
  z-index: 0;
  font-size: 28rpx;
}
.tips input {
  padding: 10rpx 0 10rpx 10rpx;
  background: #f2f2f2;
  border-radius: 6px;
  font-size: 28rpx;
}
.name-phone>view {
  width: 50%;
  display: flex;
  align-items: center;
}
.items {
  color: #000;
  margin: 0;
  display: flex;
  padding: 10rpx 0;
  border-radius: 0;
  align-items: center;
}

.items text {
  padding-right: 12rpx;
  text-align: left;
  color: #1a1a1a;
  font-size: 28rpx;
}
.time-picker {
  height: 62rpx;
  text-align: left;
  font-size: 28rpx;
  color: #424242;
  flex: 1;
  line-height: 62rpx;
  background: #f2f2f2;
  padding-left: 10rpx;
}
.items .min_icon {
  display: flex;
  align-items: center;
  margin-left: auto;
  margin-right: 20rpx;
}
.items .min_icon image {
  width: 12rpx;
  margin-left: 30rpx;
}
.bd {
  padding: 0 10rpx;
}
.tot {
  padding: 0 26rpx 26rpx 26rpx;
  font-size: 30rpx;
}
.color-64B {
  color: #29c572 !important;
}
.bd .items {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.bd .tips {
  font-size: 24rpx;
  color: #888;
}
.bd .items form {
  width: 25%;
  font-size: 32rpx;
}
.bd .items button {
  display: flex;
  align-items: center;
  width: 134rpx;
  height: 134rpx;
  border-radius: 50%;
  background: #d2d2d2;
  position: relative;
  overflow: visible;
  justify-content: center;
}
.bd .items .icon_box_text {
  text-align: center;
  font-size: 30rpx;
  margin: 24rpx 0;
}
.bd .items button > view:first-of-type {
  width: 32rpx;
  height: 32rpx;
  border: 1px solid #d0d0d0;
  background: #fff;
  border-radius: 50%;
  display: flex;
  align-items: center;
  margin-right: 28rpx;
  position: absolute;
  right: -30rpx;
  bottom: 0;
}
.bd .items button image {
  width: 110rpx;
  height: 110rpx;
}
.bd .items button > view .icon_box {
  margin: 0 auto;
}
.listbox-btn {
  position: fixed;
  left: 50%;
  bottom: 0;
  transform: translateX(-50%);
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  padding-top: 24rpx;
}
.footer-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #fff;
  border-radius: 20px;
}
.footer-btn image {
  width: 306rpx;
}
.color_red{
  color: #ff0000;
}

.tips-box{
  margin: 0 20rpx;
  height: 188rpx;
  border-radius: 8px;
  background: #f1f1f1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.tips-title-box{
  height: 98rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.tips-arrow-img{
  width: 48rpx;
}
.add-pop-marsk{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  z-index: 100;
}
.add-pop-box{
  position: fixed;
  top: 50%;
  left: 50%;
  width: 90%;
  height: auto;
  background: #fff;
  transform: translate(-50%, -50%);
  padding: 30rpx 10rpx;
  border-radius: 6px;
  z-index: 999;
}
.time-pop-marsk{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.4;
  z-index: 100;
}
.time-pop-box{
  position: fixed;
  top: 50%;
  left: 50%;
  width: 90%;
  height: auto;
  background: #fff;
  transform: translate(-50%, -50%);
  padding: 30rpx 10rpx;
  border-radius: 6px;
  z-index: 999;
}
.add-pop-btn{
  padding: 20rpx 30rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add-pop-btn>.add-footer-btn {
  width: 378rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #fff;
  height: 80rpx;
  border-radius: 20px;
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
}
