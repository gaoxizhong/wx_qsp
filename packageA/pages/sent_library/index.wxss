page{
  background: #f8f8f8;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.library-page-box{
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 0;
  background: #ffffff;
  border-radius: 4px;
}
.library-page-box>view.page-box-list{
  width: 33.3333333333333333%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}
.page-box-list image{
  width: 94rpx;
  height: 94rpx;
}
.page-box-list .box-list-title{
  margin-top: 12rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
}
.explan{
  font-size: 30rpx;
  padding: 20rpx 0;
  color: #a2a2a2;
}
.select-title{
  padding: 8rpx 0;
  font-size: 30rpx;
  font-weight: 600;
}
.list-item {
  padding: 30rpx 20rpx;
  white-space: normal;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  margin: 12rpx 0;
  background: #fff;
  border-radius: 6px;
  box-shadow: 0 4rpx 12rpx #00000020;
}
.list-item .lt {
  flex: 1;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  color: #666;
  position: relative;
}
.list-item .lt .name {
  font-size: 30rpx;
  color: #333;
  height: auto;
  line-height: 60rpx;
  font-weight: 700;
  text-align: center;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.name-image{
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 8rpx;
}
.name-image >image{
  width: 40rpx;
  height: 38rpx;
}
.name-text {
  padding-top: 6rpx;
  font-size: 26rpx;
  color: #959393;
}
.right-cang {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 12rpx;
  width: auto;
  padding: 12rpx 18rpx;
  background: #5ecf92;
  color: #fff;
  font-size: 26rpx;
  border-radius: 8px;
}
.rt-juli{
  color: #c2c2c2;
  font-size: 26rpx;
  text-align: center;
}
.is_dingdan_box{
  width: 73%;
  border-radius: 6px;
  background: #fff;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.is_dingdan_mrak{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.8;
}
  