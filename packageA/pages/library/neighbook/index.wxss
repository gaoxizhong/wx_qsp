page{
  background:#ececec;
}
.b-view{
  width:100%;
  height:120rpx;
  box-sizing: border-box;
  padding: 20rpx;  
  background: #fff;
}
.b-view-box{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
}
.inner-layer{
  width:100%;
  height:100%;
  overflow: hidden;
  position: relative;
  border-radius: 6px;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.inner-layer>image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
}
.view-box-iamge{
  width: 60rpx;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}
.view-title{
  font-size:26rpx;
  flex:1;
  padding-left:20rpx;
}
.view-box-iamge image{
  width: 60rpx;
  height:50rpx;
}
.create-library{
  width: 262rpx;
  height: 262rpx;
  overflow: hidden;
}
.create-library image{
  width: 100%;
  height: 100%;
}
.create-title{
  font-size: 32rpx;
  font-weight: 700;
}
.desc-text{
  font-size: 22rpx;
  color: #777777;
  margin-top: 12rpx;
}
.library-list {
  width: 100%;
  height: calc( 100% - 80rpx );
  background-color: #fff;
  box-sizing: border-box;
  font-size: 30rpx;
  z-index: 5;
}

.library-list1 scroll-view {
  white-space: nowrap;
}
.library-list1 .list-item {
  padding: 20rpx;
  white-space: normal;
  box-sizing: border-box;
  background: #fff;
  margin: 20rpx 0;
}
.library-list1 .list-item .lt {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666;
  position: relative;
  height: auto;
}
.library-list1 .list-item .lt .lt-left{
  width: 68%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.library-list1 .list-item .lt .lt-left .lt-image{
  width: 94rpx;
  height: 94rpx;
  border-radius: 50%;
  overflow: hidden;
}
.library-list1 .list-item .lt .lt-left .lt-image image{
  width: 100%;
  height: 100%;
}
.library-list1 .list-item .lt .lt-left .lt-text{
  margin-left: 20rpx;
}
.library-list1 .list-item .lt .name {
  font-size: 30rpx;
  color: #333;
  height: auto;
  font-weight: 700;
}
.library-list1 .list-item .lt .lt-right{
  font-size: 26rpx;
  text-align: right;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.library-list1 .seemap {
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 4rpx;
  border: 2rpx solid #666;
  border-radius: 20rpx;
  padding: 0 16rpx;
}

.sousuo-box{
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  padding: 0 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.sousuo-box .input-box{
  height: 60rpx;
  width: 100%;
  background: #dcdcdc;
  border-radius: 10px;
  overflow: hidden;
}
.sousuo-box .input-box input{
  height: 100%;
  width: 100%;
  background: #dcdcdc;
  padding-left: 20rpx;
  text-align: center;
  font-size: 28rpx;
}
.btn-box{
  width: 112rpx;
  height: 60rpx;
  font-size: 28rpx;
  background: #fcab53;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 9999;
}
.rt{
  display: flex;
  align-items: center;
  margin-top: 16rpx;
}
.rt .score{
  width: 192rpx;
  height: auto;
}
.rt .score:nth-of-type(2){
  margin-left: 46rpx;
}
.rt .score:nth-of-type(3){
  margin-left: 46rpx;
}
.rt-title-box{
  padding: 12rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.rt-image-box{
  width: 192rpx;
  height: 240rpx;
  border-radius: 6px;
  overflow: hidden;
}
.rt-image-box image{
  width: 100%;
  height: 100%;
}
.rt-title-box .rt-title-text{
  font-size:25rpx;
  font-size:30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.rt-title-bg {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.btn-bg{
  font-size: 20rpx;
  display: inline-block;
  background: #4abf81;
  color: #fff;
  border-radius: 10px;
  padding: 2rpx 4rpx;
  margin-right: 8rpx;
}
.btn-jiage{
  font-size: 26rpx;
}
.right-cang{
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.right-cang image{
  width:32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}




.class_box{
  height: 262rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 20rpx 0 20rpx;
  background: #fff;
  margin-top: 20rpx;
}
.box-right{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: space-between;
  height: 100%;
}
.my-attention,.my-group{
  font-size: 28rpx;
  width: 436rpx;
  height: 124rpx;
}
.my-attention image,.my-group image{
  width: 100%;
  height: 100%;
}
.set-cover {
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: fixed;
  background-color: black;
  top: 0;
  left: 0;
  overflow: hidden;
  z-index: 9999;
}
.set-toast-box {
  width: 77.6%;
  height: auto;
  background-color: white;
  opacity: 1;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  border: 1rpx solid white;
  border-radius: 5rpx;
  box-sizing: border-box;
}
.set-input-num {
  flex: 1;
  display: flex;
  height: 60rpx;
  background-color: #b4b4b4;
  font-size: 30rpx;
  color: #FFFFFF;
  font-family: PingFang-SC-Regular;
  text-align: center;
  margin-right: 30rpx;
  margin-left: 10rpx;
}
.set-t-b{
  margin-top: 35rpx;
  height: 90rpx;
  line-height: 90rpx;
  width: 100%;
  font-size: 30rpx;
  color: #db1010;
  position: relative;
}

.set-title-input{
  display: flex;
  align-items: center;
  padding: 46rpx 36rpx 20rpx 36rpx;
  font-size: 30rpx;
}
.password-instruct{
  font-size: 28rpx;
  color: #4AC081;
  padding: 0 36rpx;
}
.hover-set-t{
  color: #777777;
}
.set-t-b:after{
  width: 100%;
  height: 1px;
  content: '';
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  position: absolute;
  top: 0;
  left: 0;
  background: #777777;
}