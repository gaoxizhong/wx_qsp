page{
  background:#ececec;
}
.create-library{
  background: #dddddd;
  border-radius: 6px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 120rpx;
  background: #fff;
}
.create-title{
  font-size: 32rpx;
  font-weight: 700;
}
.desc-text{
  font-size: 22rpx;
  color: #777777;
  margin-top: 12rpx;
}
.library-list {
  width: 100%;
  height: calc( 100% - 80rpx );
  background-color: #fff;
  box-sizing: border-box;
  font-size: 30rpx;
  z-index: 5;
}

.library-list1 scroll-view {
  white-space: nowrap;
}
.library-list1 .list-item {
  padding: 20rpx;
  white-space: normal;
  box-sizing: border-box;
  background: #fff;
  margin: 20rpx 0;
}
.library-list1 .list-item .lt {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #666;
  position: relative;
  height: auto;
}
.library-list1 .list-item .lt .lt-left{
  width: 68%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.library-list1 .list-item .lt .lt-left .lt-image{
  width: 94rpx;
  height: 94rpx;
  border-radius: 50%;
  overflow: hidden;
}
.library-list1 .list-item .lt .lt-left .lt-image image{
  width: 100%;
  height: 100%;
}
.library-list1 .list-item .lt .lt-left .lt-text{
  margin-left: 20rpx;
}
.library-list1 .list-item .lt .name {
  font-size: 30rpx;
  color: #333;
  height: auto;
  font-weight: 700;
}
.library-list1 .list-item .lt .lt-right{
  font-size: 26rpx;
  text-align: right;
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.library-list1 .seemap {
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 4rpx;
  border: 2rpx solid #666;
  border-radius: 20rpx;
  padding: 0 16rpx;
}

.sousuo-box{
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  padding: 0 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.sousuo-box .input-box{
  height: 50rpx;
  width: 100%;
  background: #dcdcdc;
  border-radius: 10px;
  overflow: hidden;
}
.sousuo-box .input-box input{
  height: 100%;
  width: 100%;
  background: #dcdcdc;
  padding-left: 20rpx;
  text-align: center;
  font-size: 28rpx;
}
.btn-box{
  width: 102rpx;
  height: 50rpx;
  font-size: 28rpx;
  background: #fcab53;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 9999;
}
.rt{
  display: flex;
  align-items: center;
  margin-top: 16rpx;
}
.rt .score{
  width: 192rpx;
  height: auto;
}
.rt .score:nth-of-type(2){
  margin-left: 46rpx;
}
.rt .score:nth-of-type(3){
  margin-left: 46rpx;
}
.rt-title-box{
  padding: 12rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.rt-image-box{
  width: 192rpx;
  height: 240rpx;
  border-radius: 6px;
  overflow: hidden;
}
.rt-image-box image{
  width: 100%;
  height: 100%;
}
.rt-title-box .rt-title-text{
  font-size:25rpx;
  font-size:30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.rt-title-bg {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.btn-bg{
  font-size: 20rpx;
  display: inline-block;
  background: #4abf81;
  color: #fff;
  border-radius: 10px;
  padding: 2rpx 4rpx;
  margin-right: 8rpx;
}
.btn-jiage{
  font-size: 26rpx;
}
.right-cang{
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.right-cang image{
  width:32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}
.cover {
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: fixed;
  background-color: black;
  top: 0;
  left: 0;
  overflow: hidden;
  z-index: 9999;
}
.input-num {
  display: flex;
  width: 325rpx;
  height: 60rpx;
  border: 1px solid #DCDCDC;
  border-radius: 12px;
  background-color: #b4b4b4;
  margin-top: 30rpx;
  margin-left: 20%;
  font-size: 30rpx;
  color: #FFFFFF;
  font-family: PingFang-SC-Regular;
  text-align: center;
}
.t-b {
  margin-top: 35rpx;
  background-image: linear-gradient(to right, #4AC081 , #6cd67f);
  border-radius: 20px;
  height: 55rpx;
  line-height: 55rpx;
  width: 144rpx;
  font-size: 26rpx;
  color: #fff;
}
.class_box{
  display: flex;
  align-items: center;
}
.my-attention,.my-group{
  font-size: 28rpx;
  background: #fff;
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.my-attention{
  margin: 0 16rpx;
  background: #ff0000;
  color: #fff;
}

  
  
  