page{
  background: #f2f2f2;
  padding: 10rpx 26rpx;
  width: auto;
}
.see-list-box{
  margin:20rpx 0;
  background: #fff;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 38rpx 20rpx;
  border-radius: 6px;

}
.list-box-item{
  flex: 1;
  display: flex;
  justify-items: flex-start;
  align-items: center;
}
.list-box-item image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}