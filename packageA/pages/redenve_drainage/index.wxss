page{
  padding: 30rpx;
  box-sizing: border-box;
}
.top-announcement-box{
  width: 100%;
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.announcement-img-box{
  width: 52rpx;
  height: 42rpx;
}
.announcement-img-box image{
  width: 100%;
  height: 100%;
}
.module-box{
  padding: 64rpx 0;
}
.select-box{
  width: auto;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
}
.select-box image{
  width: 106rpx;
  height: 106rpx;
}
.footer-btn{
  width: 100%;
  padding: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}

.footer-btn>view{
  width: 282rpx;
  text-align: center;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 30rpx;
  background-color: #438dbd;
  color: #fff;
  border-radius: 20px;
}
.btn-hover{
  opacity: .8;
}
.circle {
  padding: 14rpx 40rpx;
  background: #fff;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  box-shadow: 0 0 6px 2px #00000020;
  border-radius: 6px;
}
.shop-centent {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.shop-centent-title {
  height: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: stretch;
}
.shop-img {
  width: 144rpx;
  height: 144rpx;
  overflow: hidden;
}
.shop-jottings {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  padding-left: 20rpx;
  overflow: hidden;
}
.shop-img .shop-img-width {
  width: 100%;
  height: 100%;
}
.shop-jottings .text, .shop-jottings .text.cool {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.shop-jottings .text {
  font-size: 28rpx;
  height: auto;
  overflow: hidden;
}
.circle-head {
  display: flex;
  align-items: center;
  position: relative;
}
.discount {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  margin-top: 12rpx;
}
.discount-price {
  color: #D55B06;
}
.coupon-list-items {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 0;
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  border-radius: 10px;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  margin: 24rpx 0;
  position: relative;
  box-shadow: 0 0 20rpx 2rpx #00000020;
}
.coupon-list-items{
  background-image: url(data:image/png;base64,);
}
.listitems-ri {
  padding: 16rpx 80rpx 16rpx 14rpx;
}
.listitems-le-title, .calendar-title, .calendar-con, .shops-name, .listitems-ri .coupon-title, .listitems-ri .ri-price-curr, .listitems-ri .coupon-view, .listitems-ri .start-end-time {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.listitems-le-title {
  font-size: 24rpx;
  color: #fff;
  text-align: left;
}
.calendar-box {
  width: 174rpx;
  height: 80rpx;
  box-sizing: border-box;
  position: absolute;
  left: 50%;
  top: 60rpx;
  transform: translateX(-50%);
}
.shops-name {
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  bottom: 28rpx;
  transform: translateX(-50%);
  width: 100%;
  padding: 0 14rpx;
  box-sizing: border-box;
}
.calendar-con {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 42rpx;
  color: #fff;
}
.listitems-rititle-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.listitems-ri>view {
  box-sizing: border-box;
}
.ri-price-curr, .coupon-view {
  font-size: 24rpx;
  margin-top: 10rpx;
}
.listitems-ri>view {
  box-sizing: border-box;
}
.start-end-time {
  position: absolute;
  bottom: 4rpx;
  left: 14rpx;
  font-size: 22rpx;
  color: #c9cbcf;
}
.listitems-rititle-box .coupon-title {
  color: #000;
}
.again-box{
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  justify-content: flex-end;
  align-self: center;
}
.again-btn{
  display: inlie-black;
  padding: 0 32rpx;
  height: 54rpx;
  line-height: 54rpx;
  background: #f19149;
  font-size: 26rpx;
  color: #fff;
  border-radius: 20px;
}

.sheng-bg image{
  width: 100%;
  height: 100%;
}
.is_limit_layer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
  z-index: 9999;
}
.is_limit_box {
  width: 60%;
  padding: 30rpx 60rpx;
  background: #fff;
  border-radius: 6px;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
}
.limit_box_title {
  width: 100%;
  font-weight: 600;
  padding-bottom: 10rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.limit_btn_box {
  margin-top: 12rpx;
  padding: 20rpx 0;
}
.limit_box_label {
  display: flex;
  padding: 16rpx 0;
  position: relative;
}
.limit_box_label::after{
  content: '';
  height: 1px;
  width: 100%;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  position: absolute;
  bottom: 0;
  left: 0;
  background: #b6b6b6;
}
.limit_box_name {
  padding: 0 12rpx;
  flex: 1;
}
.limit_btn {
  width: auto;
  padding: 12rpx 30rpx;
  font-size: 30rpx;
  color: #fff;
  background: #438dbd;
  border-radius: 20px;
  text-align: center;
  margin: 0 auto;
}
.btn-hover{
  opacity: .8;
}
.buy-money-view {
  width: 100%;
  text-align: center;
  font-size: 42rpx;
  padding-bottom: 26rpx;
  position: relative;
}
.way-box {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
}
.the-view-box {
  font-size: 30rpx;
  padding-top: 30rpx;
}
.limit_btn_box {
  margin-top: 12rpx;
  padding: 20rpx 0;
}
.buy-money-view::after{
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  position: absolute;
  left: 0;
  bottom: 0;
  background: #b6b6b6;
}
.way-title {
  font-size: 28rpx;
  color: #949292;
}
.way-name {
  font-size: 30rpx;
  color: #333;
}
.promlines_btn {
  width: 266rpx;
  height: 66rpx;
  line-height: 66rpx;
  text-align: center;
  background: #2477fa;
  color: #fff;
  border-radius: 10px;
  margin: 0 auto;
}