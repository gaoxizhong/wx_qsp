page{
  font-size: 30rpx;
  box-sizing: border-box;
}
.filter-list-box{
  padding: 28rpx 0 14rpx 0;
  box-sizing: border-box;
  box-shadow: 0 0 20rpx 0 #00000015;
  border-radius: 8px;
}
.filter-list-th{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 600;
  color: #000;
}
.filter-list-tr{
  width: 100%;
  height: 106rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #000;
  margin: 14rpx 0;
}
.filter-list-tr-image{
  width: 102rpx;
  height: 102rpx;
  border-radius: 50%;
  overflow: hidden;
}
.filter-list-tr-image>image{
  width: 100%;
  height: 100%;
}
.filter-list-th>view,.filter-list-tr>view{
  text-align: center;
}
.filter-list-tr>view.filter-list-tr-name{
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.cell__hd_box{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
}
.cell__hd_box:after {
  content: '';
  display: inline-block;
  width: 8px;
  height: 8px;
  border-top: 1px solid #656565;
  border-right: 1px solid #656565;
  position: absolute;
  top: 50%;
  right: 30rpx;
  transform: rotate(45deg) translateY(-50%);
}