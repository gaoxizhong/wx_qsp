page{
  box-sizing: border-box;
  padding: 0 16rpx;
}
.title-box{
  width: 100%;
  height: 88rpx;
  background: #ffddb3;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
}
.title-box>view,.tab_tr>view.text{
  display: flex;
  justify-content: center;
  align-items: center;
}
.left-ri,.tr_td1{
  width: 140rpx;
}
.tr_td2{
  flex: 1;
}
.tab_tr{
  width: 100%;
  position: relative;
  height: auto;
  padding: 28rpx 0;
  background-color: #FFFFFF;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.tab_tr_box>view.tab_tr:nth-child(even){
  background: #ffddb3;
}
.tr_td2_btn{
  padding: 10rpx 16rpx;
  background: #fea345;
  color: #fff;
  text-align: center;
  font-size: 24rpx;
  border-radius: 6px;
  margin-left: 12rpx;
}