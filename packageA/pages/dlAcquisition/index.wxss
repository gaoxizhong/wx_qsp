page {
  font-family: PingFangSC-Regular,PingFangSC;
  color: #333;
  background: #f2f2f2;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.bac-box {
  padding: 30rpx 20rpx;
  height: auto;
  margin: 0 30rpx;
  box-sizing: border-box;
  border-radius: 14px;
  background: #fff;
}
.dl-bothinfo-box{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
  font-size: 30rpx;
}
.dl-itemsinfo-box{
  width: 208rpx;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.dl-itemsinfo-box>image{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
}
.dl-itemsinfo-box>.dl-text{
  font-size: 28rpx;
  font-weight: 600;
  margin-top: 10rpx;
}
.bothinfo-z-box{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}
.bothinfo-z-img{
  width: 58rpx;
  height: 58rpx;
  margin: 4rpx 0;
}
.jfdatainfo-box{
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  margin-top: 30rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.jfdatainfo-box-l{
  text-align: left;
}
.jfdatainfo-box-r{
  text-align: right;
}
.jfdata-text{
  color: #999;
}
.createMerchantBuy-ChooseAmount-box {
  background: #fff;
  font-size: 30rpx;
  border-radius: 8px;
  padding: 20rpx 0;
  box-sizing: border-box;
  margin: 30rpx;
}
.chooseAmount-title-box {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.chooseAmount-items-box {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  box-sizing: border-box;
  padding: 0 10rpx;
}
.chooseAmount-title-img {
  width: 34rpx;
  height: 34rpx;
}
.chooseAmount-title-text {
  padding-left: 20rpx;
  font-weight: 600;
}
.chooseAmount-items-box>view {
  width: 50%;
  box-sizing: border-box;
  padding: 10rpx;
}
.chooseAmount-items-box>view .chooseIemsList {
  box-sizing: border-box;
  border-radius: 6px;
  border: 1px solid #dcdcdc;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  height: 166rpx;
}
.chooseAmount-items-box>view .chooseIemsList.selectMoney {
  border: 1px solid #ff9005;
  color: #ff9005;
}
.chooseIemsList>view {
  flex: 1;
  font-size: 28rpx;
}
.chooseIemsList-img {
  width: 48rpx;
  height: 48rpx;
}
.chooseIemsList .chooseIemsList-input {
  text-align: center;
}
.infoPlate-box {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 40rpx 0;
}

.infoPlate-Push-box {
  width: 336rpx;
  height: 112rpx;
  position: relative;
}
.infoPlate-Push-box>image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
}
.infoPlate-text{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  font-size: 32rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
  font-weight: 600;
  color: #fff;
  text-align: center;
}

.integral-transaction {
  padding: 0;
  box-sizing: border-box;
  margin: 20rpx 30rpx;
  border-radius: 8px;
  background: #fff;
  overflow: hidden;

}
.jdt-bgbox{
  width: 100%;
  height: auto;
  position: relative;
}
.jdt-sj-box{
  width: 100%;
  height: auto;
  position: relative;
}
.jdt-bgbox>view{
  position: absolute;
  top: 0;
  left: 0;
}
.jdt-bgbox>view>image{
  width: 660rpx;
  height: 68rpx;
}
.jdt-bgbox2{
  overflow: hidden;
}
.transac-img {
  width: 100%;
  height: 138rpx;
}
.transac-img>image {
  width: 100%;
  height: 100%;
}
.taskCard-box{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 0;
  height: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
}
.taskCard-box>image{
  width: 192rpx;
  height: 32rpx;
}
.task-instruction-box{
  width: 100%;
  padding: 20rpx 12rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.task-instruction-box>view{
  display: flex;
  align-items: flex-start;
  margin: 10rpx 0;
}
.task-instruction-box>view>image{
  width: 38rpx;
  height: 38rpx;
}
.task-instruction-box>view>text{
  flex: 1;
  padding-left: 10rpx;
}
.jftask-jdt-box{
  padding: 0 20rpx;
  height: 180rpx;
  box-sizing: border-box;
  margin: 20rpx 30rpx;
  border-radius: 8px;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.jdt-sj-box1{
  width: 100rpx;
  height: 86rpx;
  position: absolute;
  top: -90rpx;
  transform: translateX(-50rpx);
}
.jdt-sj-box1>image{
  width: 100%;
  height: 100%;
  z-index: 0;
}
.sj-box1-text{
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  font-weight: 600;
  color: #fff;
  box-sizing: border-box;
  padding-bottom: 12rpx;
}
.jftask-btn-box{
  padding: 20rpx 40rpx;
  box-sizing: border-box;
  margin-top: 40rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.jftask-btn-box>button{
  width: 306rpx;
  height: 90rpx;
  border: none;
  border-radius: 20px;
  overflow: hidden;
}
.jftask-btn-box>button>image{
  width: 306rpx;
  height: 90rpx;
}
.hoverClass{
  opacity: 0.8;
}
.jzsj-box{
  width: 100%;
  margin-top: 14rpx;
  font-size: 28rpx;
  color: rgb(250, 0, 0);
}
.cyrs-box{
  width: 100%;
  margin-top: 14rpx;
  font-size: 28rpx;
  color: #7a7a7a;
}
.jyhs-page-box1 {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.jyhs-page-box1>image {
  width: 100%;
  box-shadow: 0 1px 10px #00000020;
}


