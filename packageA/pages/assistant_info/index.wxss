.indexbg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 122rpx;
  overflow: hidden;
  z-index: -1;
}
.indexbg>image {
  width: 100%;
  height: 100%;
}
.kapian-box {
  height: auto;
  z-index: 100;
  margin: 0 20rpx;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: hidden;
  background: #fff;
  padding: 40rpx 60rpx;
  box-shadow: 0 0 10px 1px #00000020;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  margin-top: 20rpx;
}
.kapian-box>view{
  text-align: center;
  flex: 1;
}
.list-box{
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 60rpx;
}
.list-items{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  position: relative;
  font-size: 28rpx;
  padding: 15rpx 0;
}
.list-items::after{
  content: '';
  width: 100%;
  height: 1px;
  transform: translateY(.5);
  background: #dbdada;
  position: absolute;
  left: 0;
  bottom: 0;
}
.items-1{
  width: 92rpx;
  height:100%;
  text-align: center;
  line-height: 80rpx;
}
.items-2{
  width: 92rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.items-2>image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.items-3{
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.items-4{
  width: 270rpx;
  text-align: center;
}
.footer-box{
  padding: 22rpx 35rpx;
  background: #fff;
  font-size: 28rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  position: fixed;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bott-box-btn {
  width: auto;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 20px;
  background: #4ac081;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  padding: 0 40rpx;
}
.no-ranklist-box{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  text-align: center;
  color: #8a8a8a;
}
/* ===================================== */
.infoPlate-box {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 40rpx 0;
}
.infoPlate-Push-box {
  width: 442rpx;
  height: 112rpx;
  position: relative;
}
.infoPlate-Push-box>image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
}
.infoPlate-text {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  font-size: 32rpx;
  padding-top: 24rpx;
  box-sizing: border-box;
  font-weight: 600;
  color: #fff;
  text-align: center;
}
.hoverClass{
  opacity: 0.8;
}
/* ====================== */
.is-pop-mark {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.4;
  z-index: 9999;
}
.is-pop-box {
  width: 650rpx;
  height: 378rpx;
  background: #fff;
  border-radius: 6px;
  padding: 12rpx 28rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  box-sizing: border-box;
}
.is-pop-listbox {
  width: 100%;
  overflow-y: scroll;
}
.newbtn-area {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #fff;
}
.newbtn-area>button {
  line-height: 1;
  width: 450rpx;
  border-radius: 20px;
  background: #4ac081;
  font-size: 30rpx;
  color: #fff;
  padding: 20rpx 30rpx;
}
.infoItem-title {
  font-size: 30rpx;
  font-weight: bold;
}
.items-list {
  width: 100%;
  height: auto;
  position: relative;
  box-sizing: border-box;
}
.infoItem-list-box {
  margin: 20rpx 0;
  padding: 20rpx 16rpx;
  background: #fff;
  border-radius: 8px;
  display: flex;
  align-items: center;
  box-shadow: 0 0 2px #00000020;
}
.index-list-img {
  width: 76rpx;
  border-radius: 50%;
}
.list-titletext {
  flex: 1;
  padding-left: 20rpx;
  font-size: 30rpx;
}
.list-top-sele {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.list-top-sele>image {
  width: 100%;
}