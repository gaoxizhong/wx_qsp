  position: relative;
}
.scaleY-1{
  transform: scaleY(1);
  transition:  0.5s;
}
.scaleY-2 {
  transform: scaleY(0.9);
  transition: 0.5s;
}
.poster-view-box swiper.swiper .swiper-item-view image {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
.image-box{
