/* 隐藏滚动条 */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.poster-view-box{
  box-sizing: border-box;
  width: 100%;
  height: 420rpx;
}
.vol-bg-box {
  width: 100%;
  height: 420rpx;
  position: relative;
}
.vol-bg-box>image.vol-bg-img {
  width: 100%;
  height: 100%;
