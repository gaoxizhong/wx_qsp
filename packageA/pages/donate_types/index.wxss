}
.layer {
  width: 100vw;
  height: 100vh;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  z-index: 999;
}
.cover {
  width: 100%;
  height: 100%;
  opacity: 0.5;
  position: fixed;
  background-color: black;
  top: 0;
  left: 0;
  overflow: hidden;
  z-index: 9999;
}
.layer .cover {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 3;
  background: #333;
}
.layer .con {
  position: relative;
  height: 100vh;
  z-index: 4;
  text-align: center;
  font-size: 36rpx;
}
.layer .con image {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.layer .con .txt {
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 30rpx;
  border-radius: 50%;
  position: fixed;
  left: 50%;
  bottom: 20rpx;
  text-align: center;
  background: #f33;
  color: #fff;
  transform: translate(-50%, 0);
}
    