page{
  box-sizing: border-box;
  padding: 20rpx;
}
.list-box {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}
.list-box>view.list-items {
  width: 50%;
  height: auto;
  margin: 23rpx 0;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.list-box>view.list-items .list-items-1 {
  width: 100%;
  height: 474rpx;
  box-sizing: border-box;
  position: relative;
  border-radius: 6px;
  overflow: hidden;
  box-shadow: 0rpx 0rpx 4px 2px#00000020;
}
.list-box>view.list-items .list-items-1 image {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}