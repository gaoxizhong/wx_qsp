page {
  font-family:PingFangSC-Regular,PingFangSC;
  box-sizing: border-box;
  padding-top: 32rpx;
  position: relative;
}
.swiper-container {
  height: 210rpx;
  z-index: 100;
  margin: 0 30rpx;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: hidden;
}
.swiper-container swiper {
  height: 210rpx;
}
.swiper-container image {
  width: 100%;
  height: 210rpx;
}
.class-module{
  height: auto;
  margin: 40rpx 30rpx 10rpx 30rpx;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.class-module>view.items{
  width: 214rpx;
  height: 210rpx;
  overflow: hidden;
  border-radius: 6px;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.class-module>view.items:nth-of-type(3n+1){
  margin-left: 0;
}
.class-module>view.items image {
  width: 100%;
  height: 100%;
}
.page-bottom-title{
  position: fixed;
  bottom: 0;
  left: 0;
}
.title_text{
  font-size: 24rpx;
  color: #fff;
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  box-sizing: border-box;
  padding: 0 8rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 2;
  text-align: right;
}