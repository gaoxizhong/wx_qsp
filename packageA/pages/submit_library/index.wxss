page{
  background: #f8f8f8;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.submit-library-title-box{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
}
.submit-library-title-btn{
  width: 140rpx;
  text-align: right;
  font-size: 28rpx;
  color: #ce0000;
}
.list-item {
  padding: 30rpx 20rpx;
  white-space: normal;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  margin: 12rpx 0;
  background: #fff;
  border-radius: 6px;
  box-shadow: 0 4rpx 12rpx #00000020;

}
.list-item .lt {
  flex: 1;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  color: #666;
  position: relative;
}
.list-item .lt .name {
  font-size: 30rpx;
  color: #333;
  height: auto;
  line-height: 60rpx;
  font-weight: 700;
  text-align: center;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.name-text {
  padding-top: 6rpx;
  font-size: 26rpx;
  color: #959393;
}
.name-image {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 8rpx;
}
.name-image >image {
  width: 40rpx;
  height: 38rpx;
}
.right-cang {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 12rpx;
  width: auto;
  padding: 12rpx 18rpx;
  background: #5ecf92;
  color: #fff;
  font-size: 26rpx;
  border-radius: 8px;
}
.rt-juli {
  color: #c2c2c2;
  font-size: 26rpx;
  text-align: center;
}
.items {
  color: #000;
  margin: 0;
  display: flex;
  padding: 30rpx 0;
  border-radius: 0;
  align-items: center;
}
.fangshi {
  display: flex;
  flex-wrap: nowrap;
}
.fangshi-one {
  display: flex;
  padding: 20rpx 30rpx;
  background: #dadada;
  margin-top: 38rpx;
  border-radius: 12rpx;
  position: relative;
}
.tot1 {
  padding: 26rpx 0;
  font-size: 30rpx;
}
.expec-obtain-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
}
.items>text {
  padding-right: 12rpx;
  text-align: left;
  color: #1a1a1a;
  font-size: 30rpx;
  font-weight: 700;
}
.fangshi>view {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 60rpx;
  border: 1px solid #29c572;
  border-radius: 6px;
  color: #333;
  margin: 0 20rpx;
}
.fangshi>view.hove {
  background: #29c572;
  color: #fff;
}
.fangshi-one-1:before {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: -12px;
  left: 80rpx;
  border-right: solid 12px transparent;
  border-left: solid 12px transparent;
  border-bottom: solid 12px #dadada;
}
.fangshi-one>view {
  flex: 1;
  font-size: 28rpx;
}
.fangshi-one .fangshi-text {
  color: #333;
  padding: 0 14rpx;
}
.fangshi-one>view input {
  padding: 10rpx;
  background: #fff;
  border-radius: 8px;
  text-align: center;
  margin: 12rpx 0;
}
.activity-imgs {
  padding: 24rpx 24rpx 24rpx 6rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.activity-imgs view {
  width: 124rpx;
  height: 124rpx;
  position: relative;
  border: 2rpx solid #999;
  border-radius: 12rpx;
  background: #d1d1d1;
}
  
.activity-imgs image {
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx 24rpx 10rpx 0;
}
.activity-imgs view image {
  width: 84rpx;
  height: 84rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.activity-imgs image {
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx 24rpx 10rpx 0;
}
.expec-obtain-box>text {
  color: #29c572;
  padding-left: 40rpx;
}
.fangshi-one-1:before {
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  top: -12px;
  left: 80rpx;
  border-right: solid 12px transparent;
  border-left: solid 12px transparent;
  border-bottom: solid 12px #dadada;
  }
.personal-info-box {
  width: 100%;
  padding: 24rpx 0;
  box-sizing: border-box;
}
.bd {
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.collec-book-title {
  font-size: 32rpx;
  font-weight: 600;
}
.name-phone {
  display: flex;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.garden-box {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  margin-top: 12rpx;
}
.collec-book-title>text {
  font-size: 28rpx;
  font-weight: 100;
}
.name-phone>view {
  width: 50%;
  display: flex;
  align-items: center;
}
.types-i {
  margin: 18rpx 0;
}
.types-input {
  flex: 1;
  padding: 10rpx 0 10rpx 20rpx;
  z-index: 0;
  font-size: 26rpx;
  background: #eaeaea;
}
.bd .btn-send {
  width: 100%;
  background: #fff;
  padding: 20rpx 34rpx;
  position: fixed;
  left: 50%;
  bottom: 0;
  transform: translateX(-50%);
  box-sizing: border-box;
}
  
.bd .btn-send button {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20px;
  border: none;
  background-image: linear-gradient(to right, #2eae84 , #6cd67f);
  background: #29c572;
  font-size: 30rpx;
  color: #fff;
}
  
  
  
  