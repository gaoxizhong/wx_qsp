page {
  font-family: PingFangSC-Regular,PingFangSC;
  color: #333;
  background: #f2f2f2;
  padding: 30rpx 0;
  box-sizing: border-box;
}
.integral-transaction {
  padding: 0;
  box-sizing: border-box;
  margin: 0 30rpx;
  border-radius: 8px;
  background: #fff;
  overflow: hidden;
}
.acquisition-module-box {
  width: 100%;
  padding: 10rpx 20rpx 30rpx;
  box-sizing: border-box;
  position: relative;
}
.being-conduct-box {
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  border-radius: 8px;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
  margin-top: 10rpx;
}
.acquisition-module-box::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  height: 1px;
  transform: translateY(0.5);
  background: #dddddd;
  box-sizing: border-box;
  width: 100%;
  z-index: 1;
}
.being-conduct-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
}
.being-conduct-view {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  color: #ffd508;
  padding-left: 130rpx;
  font-weight: 600;
}
.acquisitionList-box {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.acquisitionList-items-box {
  width: 100%;
  padding: 30rpx 0;
  position: relative;
}
.acquisitionList-items-t {
  box-sizing: border-box;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
}
.userList-box {
  padding: 0 20rpx;
  box-sizing: border-box;
  background: #f8f8f8;
  border-radius: 20px;
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}
.acquisitionList-items-box::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  height: 1px;
  transform: translateY(0.5);
  background: #dddddd;
  box-sizing: border-box;
  width: 100%;
  z-index: 1;
}
.acList-r-btn1{
  width: auto;
  height: auto;
  text-align: center;
  margin-top: 44rpx;
}
.acList-items-l-box {
  flex: 1;
  display: flex;
  align-items: flex-start;
}
.acList-items-r-box {
  width: auto;
  height: auto;
}
.acList-l-avatar {
  width: 78rpx;
  height: 78rpx;
  border-radius: 8px;
}
.acList-l-view {
  padding-left: 12rpx;
}
.acList-l-title {
  font-size: 30rpx;
  font-weight: 600;
}
.acList-l-ac {
  font-size: 28rpx;
  padding-top: 8rpx;
}
.acList-l-bj {
  font-size: 28rpx;
  margin-top: 12rpx;
}
.acList-l-ac text {
  color: #b2b2b2;
}
.acList-r-btn {
  width: 154rpx;
  height: 87rpx;
  position: relative;
  text-align: center;
  color: #fff;
  margin: 8rpx 0;
}
.acList-r-btn .acList-r-btn-img {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.acList-r-btn-text {
  position: absolute;
  top: 10rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 1;
  font-size: 26rpx;
  display: inline-block;
  width: 100%;
}
.userList-number-img {
  width: 360rpx;
  height: 100%;
  position: relative;
  overflow: hidden;
}
.userList-number-img>view {
  border-radius: 50%;
  width: 60rpx;
  height: 60rpx;
  vertical-align: auto;
  overflow: hidden;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.userList-number-img>view>image {
  width: 100%;
  height: 100%;
}
.userList-text {
  flex: 1;
  font-size: 28rpx;
  font-weight: 600;
  text-align: right;
}
.acList-r-btn1>image {
  width: 183rpx;
}