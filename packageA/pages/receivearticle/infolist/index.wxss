page{
  background: #eaeaea;
  padding: 26rpx 20rpx;
  box-sizing: border-box;
}
.datalist-items{
  background: #fff;
  font-size: 28rpx;
  border-radius: 6px;
  padding: 0 34rpx 0 10rpx;
  margin: 12rpx 0;
}
.items-title{
  border-bottom: 1rpx solid #d2d2d2;
  font-size: 26rpx;
  padding: 20rpx 0 26rpx 15rpx;
}
.items-info{
  padding: 20rpx 0 20rpx 15rpx;
}
.items-info-top{
  display: flex;
  align-items: flex-start;
  justify-content: flex-start;
  box-sizing: border-box;
}
.info-imges-box{
  width: 146rpx;
  height: 146rpx;
  overflow: hidden;
}
.info-imges-box>image{
  width: 100%;
  height: 100%;
}
.info-right-info{
  height: 100%;
  padding-left: 38rpx;
  flex: 1;

}
.pinlei{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 22rpx;
}
.chubu{
  line-height: 40rpx;
  text-align: center;
  font-size: 25rpx;
  color: #fff;
  background:#49c383;
  border-radius: 6px;
  display: inline;
  padding: 8rpx 14rpx;
}
.pinpai{
  margin-top: 12rpx;
  width: 70%;
  overflow:hidden; /*超出的文本隐藏*/
  text-overflow:ellipsis; /*溢出用省略号显示*/
  white-space:nowrap; /*溢出不换行*/
}
.items-info-bottom{
  padding-top: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.info-bottom1-btn{
  width: 162rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  background:#49c383;
  color: #fff;
  text-align: center;
  border-radius: 20px;
  margin-left: 22rpx;
}
.info-bottom-btn{
  width: 162rpx;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 26rpx;
  border: 3rpx solid #49c383;
  text-align: center;
  border-radius: 20px;
  margin-left: 20rpx;
}
.pinlei-list{
  width: 70%;
  overflow:hidden; /*超出的文本隐藏*/
  text-overflow:ellipsis; /*溢出用省略号显示*/
  white-space:nowrap; /*溢出不换行*/
}
.addlistdata{
  position: fixed;
  left: 50%;
  bottom: 40rpx;
  transform: translateX(-50%);
  width: 230rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #4bc381;
  font-size: 30rpx;
  text-align: center;
  color: #fff;
  border-radius: 20px;
}
.tel{
  width: auto;
  height:auto;
  position: fixed;
  right: 15rpx;
  bottom: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  background: #fff;
  border-radius: 8px;
  padding: 10rpx;

}
.tel image{
  width: 82rpx;
  height: 82rpx;
}