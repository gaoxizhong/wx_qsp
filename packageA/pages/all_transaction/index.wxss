page{
  font-size: 30rpx;
  box-sizing: border-box;
}
.filter-title-box{
  width: 100%;
  box-sizing: border-box;
  padding: 26rpx 30rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background: #fff;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.filter-title-top{
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.filter-title-top:after {
  content: '';
  display: inline-block;
  width: 8px;
  height: 8px;
  border-top: 1px solid #656565;
  border-right: 1px solid #656565;
  position: absolute;
  top: 50%;
  right: 10rpx;
  transform: rotate(45deg) translateY(-50%);
}
.filter-title-top.show-arr:after{
  transform: rotate(135deg) translateX(-100%);
}
.filter-title-bott{
  width: 100%;
  height: 0rpx;
  padding: 0 30rpx;
  background: #fff;
  overflow: hidden;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  top: 92rpx;
  z-index: 100;
  opacity: 0;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.filter-title-bott>view{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 10rpx 0;
}
.list-box-items{
  flex: 1;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.filter-title-bott>view text{
  width: 80rpx;
}
.filter-title-bott.show-class{
  padding: 20rpx 30rpx;
  height: auto;
  opacity: 1;
  transition:0.5s;
  -moz-transition: 0.5s; /* Firefox 4 */
  -webkit-transition: 0.5s; /* Safari and Chrome */
  -o-transition: 0.5s; /* Opera */
}
.is_address{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;

}
.filter-list-box{
  margin-top: 94rpx;
  padding: 28rpx 0 14rpx 0;
  box-sizing: border-box;
  box-shadow: 0 0 20rpx 0 #00000015;
  border-radius: 8px;
}
.filter-list-th{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 600;
  color: #000;
}
.filter-list-tr{
  width: 100%;
  height: 106rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #000;
  margin: 14rpx 0;
}
.filter-list-tr-image{
  width: 102rpx;
  height: 102rpx;
  border-radius: 50%;
  overflow: hidden;
}
.filter-list-tr-image>image{
  width: 100%;
  height: 100%;
}
.filter-list-th>view,.filter-list-tr>view{
  text-align: center;
}
.filter-list-tr>view.filter-list-tr-name{
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.cell__hd_box{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.weui-cell__hd{
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}
.fixed-bottom-box{
  width:100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 34rpx  0 20rpx 0;
  height: 150rpx;
  box-sizing: border-box;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
}
.fixed-bottom-box::before{
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
  position: absolute;
  top: 0;
  left: 0;
  background: #999;
  }
.fixed-bottom-box>view{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 28rpx;
  color: #000;
  height: 100%;
}
.fixed-bottom-btn{
  width: 128rpx;
  height: 46rpx;
  line-height: 46rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
  background: #f9c15f;
  border-radius: 20px;
}
.distance-item{
  width: 90rpx;
  height: 42rpx;
  font-size: 24rpx;
  line-height: 42rpx;
  text-align: center;
  background: #fff;
  box-shadow: 0 0 20rpx 0 #00000015;
  border-radius: 20px;
  margin-left: 24rpx;
  margin-bottom: 24rpx;

}

.distance-item.highlight-item{
  background: #4ac081;
  color: #fff;
}
.agelist-item{
  width: auto;
  padding: 4rpx 22rpx;
  font-size: 24rpx;
  background: #fff;
  box-shadow: 0 0 20rpx 0 #00000015;
  border-radius: 20px;
  margin-left: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.agelist-item.highlight-item{
  background: #4ac081;
  color: #fff;
}
.region-box{
  flex:1;
}
.region-box>input{
  margin-left: 26rpx;
  width: 76%;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 26rpx;
  padding-left: 20rpx;
  border-radius: 20px;
  background: #fff;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.masking{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99;
  background: #000;
  opacity: 0.08;
  -moz-opacity:0.08;
  -khtml-opacity:0.08;
}