page{
  box-sizing: border-box;
  padding: 10rpx 30rpx;
}
.anwer-title-box{
  width: 100%;
  padding: 20rpx 0;
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  color: #4ac081;
}
.answer-list-box{
  width: 100%;
  height: auto;
  font-size: 28rpx;
}
.answer-items{
  width: 100%;
  margin: 20rpx auto;
}
.answer-options-box{
  width: 100%;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
}
.options-list{
  margin: 20rpx 0;
  font-size: 28rpx;
}

.icon-fen{
 padding:4rpx 22rpx;
 font-size: 28rpx;
 background: #e9e9e9;
 display: inline-block;
 border-radius: 20px;
 margin-left: 20rpx;
}
.parsing-box{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #e9e9e9;
  border-radius: 6px;
}
.parsing-box-1{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #ff0000;
}
.parsing-box-2{
  padding-left: 30rpx;
  font-size: 30rpx;
  margin-top: 20rpx;
}