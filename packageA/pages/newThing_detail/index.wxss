.title-box{
  box-sizing: border-box;
  width: 100%;
  padding: 30rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.title-box-info{
  padding-left: 30rpx;
}
.title-box-time{
  font-size: 26rpx;
  color: #999;
}
.title-box-avatar{
  width: 80rpx;
  height: 80rpx;
}
.swiper-container {
  width: 100%;
  height: 420rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.swiper-container .swiper {
  height: 420rpx;
}
.swiper-container image {
  width: 100%;
  height: 420rpx;
}
.content-box{
  width: 100%;
  font-size: 30rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.imagesList-box{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.imagesList-items{
  width: 100%;
  height: 340rpx;
  margin: 30rpx 0;
  background: #fff;
  border-radius: 8px;
  overflow: hidden;
}
.imagesList-items>image {
  width: 100%;
  height: 100%;
}