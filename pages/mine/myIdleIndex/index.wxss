page{
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.myinfo-box{
  width: 100%;
  height: 166rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  position: relative;
}
.myinfo-box::after{
  content: '';
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
  position: absolute;
  left: 0;
  bottom: 0;
  background: #dadada;
}
.myinfo-l-box{
  width: auto;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.userAvatarUrl-box{
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
  overflow: hidden;
}
.userAvatarUrl-box>image{
  width: 100%;
  height: 100%;
}
.nickName{
  padding-left: 14rpx;
  font-weight: 600;
}
.myinfo-r-box{
  width: auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.myinfo-r-schb-box{
  width: 144rpx;
  height: 66rpx;
  text-align: center;
  line-height: 66rpx;
  font-size: 28rpx;
  color: #ff905f;
  border: 1px solid #ff905f;
  border-radius: 20px;
}
.btnlist-box{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 0;
}
.btnlist-title-box{
  font-size: 30rpx;
  font-weight: 600;
  margin: 10rpx 0;
}
.btnlist-info-box{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.btnlist-info-box>view {
  width: 33.33333333333%;
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  margin: 20rpx 0;
  position: relative;
}
.btnlist-info-box>view>image {
  width: 122rpx;
  height: 122rpx;
}
.info-text {
  padding-top: 10rpx;
}
.myidle-box {
  height: auto;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: initial;
  background: #fff;
  margin-top: 40rpx;
}
.myidle-box-title {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.myidle-title-l {
  display: flex;
  align-items: center;
}
.myidle-title-r {
  font-size: 32rpx;
  color: #888;
}
.myidle-title-l-b {
  width: 16rpx;
  height: 46rpx;
  border-radius: 6px;
  background: linear-gradient(to bottom,#fd8002,#feba2d);
}
.myidle-title-l text {
  padding-left: 14rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.myidle-title-r {
  font-size: 32rpx;
  color: #888;
}
.goodinfo {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
}
.goodinfo>view.goToInfo {
  width: 50%;
  height: auto;
  margin: 23rpx 0;
  padding: 0 10rpx;
  box-sizing: border-box;
}
.goodinfo>view.goToInfo {
    width: 50%;
    height: auto;
    margin: 23rpx 0;
    padding: 0 10rpx;
    box-sizing: border-box;
}
.goodinfo>view.goToInfo .good-item-view {
  width: 100%;
  position: relative;
  height: auto;
  box-sizing: border-box;
  border-radius: 6px;
  overflow: hidden;
  box-shadow: 0rpx 0rpx 4px#00000020;
}
.good-item-view image.good-item-img {
  width: 100%;
  height: 326rpx;
  justify-content: center;
  display: block;
  border-radius: 6px;
}
.good-item-view .goodname {
  font-size: 26rpx;
  color: #333;
  display: block;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  margin-top: 10rpx;
  padding: 0 10rpx;
}
.goodprice {
  font-size: 26rpx;
  color: #333;
  display: flex;
  align-items: center;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  padding: 0 10rpx;
}
.idle-avar-box {
  width: 100%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 10rpx 10rpx;
}
.goodprice .goodprice-1 {
  font-size: 24rpx;
  color: #999;
}
.goodprice .goodprice-2 {
  font-size: 32rpx;
  color: #fc6421;
  font-weight: 600;
}
.idle-avar-box>image {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
}
.idle-avar-box>text {
  margin-left: 12rpx;
  font-size: 24rpx;
  color: #999;
}