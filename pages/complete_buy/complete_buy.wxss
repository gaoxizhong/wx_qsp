page {
  font-family: PingFangSC-Regular,PingFangSC;
  color: #333;
}
.pic {
  margin: 0 auto;
  margin-top: 136rpx;
  margin-bottom: 58rpx;
  width: 160rpx;
  height: 160rpx;
}
.pic image {
  width: 100%;
  height: 100%;
}
.success {
  font-size: 40rpx;
  text-align: center;
  margin-bottom: 44rpx;
}
.status {
  font-size: 28rpx;
  color: #999;
  text-align: center;
  margin-bottom: 144rpx;
}
.btns {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.btns button {
  margin: 0;
  padding: 0;
  width: 240rpx;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 28rpx;
  color: #020202;
  border: 1px dashed #020202;
}