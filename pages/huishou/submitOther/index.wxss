.color_red {
  color: #FF0221 !important;
}

.color_blue {
  color: #64B532 !important;
}
.state_wrap {
  padding-top: 82rpx;
  text-align: center;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
}
.state_wrap image {
  display: block;
  width: 308rpx;
  height: 308rpx;
  margin: 0 auto;
  margin-bottom: 40rpx;
}

.state_wrap .txt {
  font-size: 36rpx;
  color: #333;
}

.state_wrap .msg {
  font-size: 26rpx;
  color: #CDCDCD;
  padding: 16rpx 64rpx;
  display: block;
}
.success-btn-list {
  margin-top: 82rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.success-btn-list button {
  width:278rpx;
  height:86rpx;
  line-height: 86rpx;
  color: #fff;
  font-size: 30rpx;
  background:#29c572;
  border-radius:42rpx;
  margin: 0;
}
.success-btn-list .btn-rt {
  color: #333;
  background:none;
  border: 2rpx solid #29c572;
  margin-left: 30rpx;
}
.bottom-text {
  height: 48rpx;
  width: 100%;
  position: fixed;
  bottom: 40rpx;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #64B532;
  font-size: 32rpx;
}
.bottom-text image {
  width: 42rpx;
  height: 42rpx;
}
.frequency{
  padding: 16rpx 24rpx;
  font-size: 30rpx;
}
.frequency .fre-item{
  padding: 16rpx 0;
  font-size: 30rpx;
  font-weight: 700;
  text-align: left;

}
.frequency .fre-item text{
  padding-left: 20rpx;
  color: #64B532;
}
.list-box{
  display: flex;
  flex-wrap: wrap;
  padding: 20rpx;
  border-radius: 10px;
  background: #dcdcdc;
}
.list-box .item-list{
  width: 50%;
  height: 66rpx;
  line-height: 66rpx;
  text-align: left;
  font-size: 28rpx;
  color: #333;
}
.idleList-box {
  width: 100%;
  height: auto;
}
.idleList-box-item {
  height: auto;
  border: 1px solid rgb(224, 224, 224);
  border-radius: 12rpx;
  box-shadow: 2rpx 2rpx 2rpx #fd783c,-2rpx -2rpx 1rpx rgb(233, 232, 232);
  margin: 18rpx 0;
  position: relative;
}
.circle {
  padding: 14rpx 40rpx;
  background: #fff;
  height: 100%;
  box-sizing: border-box;
  position: relative;
}
.discount-price {
  color: #D55B06;
}
.discount-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120rpx;
  height: 38rpx;
  color: #fff;
  font-size: 22rpx;
  background: #D55B06;
  border-radius: 10rpx;
  line-height: 38rpx;
  position: absolute;
  right: 20rpx;
  bottom: 17rpx;
  padding-bottom: 4rpx;
  z-index: 100;

  }
      
