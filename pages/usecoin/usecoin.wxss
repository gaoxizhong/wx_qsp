page{
  background: #fff;
  box-sizing: border-box;
}
.pages-1{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  color: #ababab;
  text-align: center;
  margin-top: 20rpx;
}
.coupon-list-items {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx 0;
  position: absolute;
  left: 30rpx;
  top: 0;
  width: 690rpx;
  height: 230rpx;
  box-sizing: border-box;
  border-radius: 10px;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  box-shadow: 0 0 20rpx 2rpx #00000020;
}
.bottom-box {
  width: 100%;
  height: 110rpx;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
  box-shadow: 0 0 10rpx 0 #00000015;
  z-index: 1;
}
.bottom-box>view {
  width: 200rpx;
  height: 76rpx;
  overflow: hidden;
  position: relative;
}
.bottom-box>view.bottom-box-ri{
  width: 370rpx;
}
.bottom-box-ri.you-box-ri{
  border-radius: 20px;
  color: #fff;
  background: #ffbd73;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.bottom-box-ri.shang-box-ri{
  width: auto;
  border-radius: 20px;
  color: #fff;
  background: #f73b3b;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.bottom-box>view>image{
  width: 100%;
  height: 100%;
}
.product-title{
  padding: 30rpx;
}
.prduct-box{
  width: 100%;
  height: auto;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.prduct-img-box{
  width: 100%;
  height: 100%;
  padding: 30rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 8px 2px #00000015;
  position: relative;
}
.prduct-img-box>image{
  height: 360rpx;
}

.citve-box>view>view.number_view {
  display: flex;
  font-size: 20rpx;
  color: #f77a6c;
  position: absolute;
  top: -14rpx;
  right: 2rpx;
  width: auto;
  height: 38rpx;
  line-height: 38rpx;
  background: #f76868;
  color: #fff;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
  padding: 0 12rpx;
}
  
.jia-jifen{
  padding: 12rpx 36rpx;
  width: auto;
  height: auto;
  color: #000;
  font-size: 30rpx;
  border-radius: 6px;
  position: fixed;
  right: 64rpx;
  bottom: 60rpx;
  opacity: 0;
  background: #fff;
  box-shadow: 0 0 10rpx 2rpx #00000020;
}
.jia-jifen2{
  padding: 12rpx 36rpx;
  width: auto;
  height: auto;
  color: #000;
  font-size: 30rpx;
  border-radius: 6px;
  position: fixed;
  left: 64rpx;
  bottom: 60rpx;
  opacity: 0;
  background: #fff;
  box-shadow: 0 0 10rpx 2rpx #00000020;
}
.meiyou-box{
  font-size:32rpx;
  text-align:center;
  width: 490rpx;
  height: auto;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  overflow: hidden;
}
.meiyou-box>image{
  width: 490rpx;
  height: 328rpx;
}
.liuyan-box{
  width: 100%;
  height: auto;
  box-sizing: border-box;
  display: flex;
  align-items: flex-start;
  padding: 30rpx;
}
.liuyan-box-left{
  width: 84rpx;
  height: 84rpx;
  border-radius: 6px;
  overflow: hidden;
}
.liuyan-box-left>image{
  width: 100%;
  height: 100%;
}
.liuyan-box-right{
  flex: 1;
  padding-left: 20rpx;
}
.dianzhang-info{
  padding: 16rpx 20rpx;
  background: #89d563;
  color: #fff;
  margin-top: 10rpx;
  position: relative;
  font-size: 26rpx;
  border-radius: 3px;
}
.dianzhang-info>image{
  width: 8rpx;
  height: 16rpx;
  position: absolute;
  left: -8rpx;
  top: 4rpx;
}
.citve-box>view.gotofujin-box{
  width: 180rpx;
  height: 126rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.gotofujin-box>image{
  width: 100%;
  height: 100%;
  z-index: 0;
  position: absolute;
  top: 0;
  left: 0;

}
.commodity-items {
  position: relative;
  height: auto;
  margin: 20rpx 0;
  box-shadow: 0 0 20rpx 0 #00000015;
}
.circle {
  padding: 14rpx 40rpx;
  background: #fff;
  height: 100%;
  box-sizing: border-box;
  position: relative;
}
.shop-centent {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.shop-centent-title {
  height: auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: stretch;
}
.shop-img {
  width: 144rpx;
  height: 144rpx;
  overflow: hidden;
}

.shop-jottings {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  padding-left: 20rpx;
  overflow: hidden;
}
.shop-img .shop-img-width {
  width: 100%;
  height: 100%;
}
.shop-jottings .text, .shop-jottings .text.cool {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.circle-head {
  display: flex;
  align-items: center;
  position: relative;
}
.discount {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  margin-top: 12rpx;
}
.discount-price {
  color: #D55B06;
}
.usecoin-nav-box{
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 15rpx;
  display: flex;
  /* flex-wrap: wrap; */
  align-items: center;
  position:fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #fff;
}
.usecoin-nav-box>view {
  border-radius: 6px;
  box-sizing: border-box;
  background: #fff;
  position: relative;
  padding: 10rpx;
}
.usecoin-nav-box .tab-view-left{
  width: 338rpx;
}
.usecoin-nav-box .tab-view-right{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.usecoin-nav-box .tab-view-right>view{
  width: 100%;
  box-sizing: border-box;
}
.usecoin-nav-box .tab-view-left image{
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
  position: relative;
}
.usecoin-nav-box .tab-view-right view image{
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
  position: relative;
}
.usecoin-nav-box>view>view.number_view {
  display: flex;
  font-size: 20rpx;
  color: #f77a6c;
  position: absolute;
  top: 2px;
  right: 6rpx;
  width: auto;
  height: 38rpx;
  line-height: 38rpx;
  background: #f76868;
  color: #fff;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
  padding: 0 12rpx;
}
.usecoin-hover{
  border: 2px solid #4ac081;
}
 /* =====================   便民服务模块功能以下   ==================== */
 .class-module {
  height: auto;
  margin: 40rpx 30rpx 10rpx 30rpx;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.class-module>view.items {
  width: 214rpx;
  height: 210rpx;
  overflow: hidden;
  border-radius: 6px;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.class-module>view.items:nth-of-type(3n+1) {
  margin-left: 0;
}
.class-module>view.items image {
  width: 100%;
  height: 100%;
}
.title_text {
  font-size: 24rpx;
  color: #fff;
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  box-sizing: border-box;
  padding: 0 8rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 2;
  text-align: right;
}
   

 /* =====================   便民服务模块功能以上   ==================== */