page {
  padding: 0 20rpx;
  padding-top:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  box-sizing: border-box;
}
.walk-through-list .item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.walk-through-list .item image {
  width: 100%;
  height: 280rpx;
  margin-bottom: 8rpx;
}
.walk-through-list .item .bottom {
  width: 100%;
  padding-bottom: 8rpx;
  margin-bottom: 24rpx;
  border-bottom: #e2e2e2 solid 3rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.walk-through-list .item .title {
  font-size: 32rpx;
  color: #333;
}
.walk-through-list .item .view-time {
  font-size: 26rpx;
  color: #999;
}