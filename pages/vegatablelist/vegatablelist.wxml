<view class='nulldata_wrap' wx:if="{{dataStatus}}"><image src='http://oss.qingshanpai.com/huanbaobi/21d3b572a908f5363be15d3149adf193.png' mode='widthFix'></image><view class='msg'>暂无数据</view></view><view class="box-list" wx:else><block wx:for="{{listData}}" wx:key="idx" wx:for-index="index"><view class="box-list-module" style="height:{{ is_circle == 1?520 : 576 }}rpx;"><view class="list-module-item"><view class="list-module-img" bind:tap="goToActivity" data-is_songcai="1" data-curidx="{{index}}" data-vegetable_id="{{item.id}}"><image src="{{item.image}}"></image></view><view class="box-list-text"><view class="list-text-title"><view>{{item.goods_name}}</view><view class="text-title-img" wx:if="{{is_circle != 1 }}"><button hover-class='none' open-type="share" data-sharetxt="{{item.goods_name}}" data-gdImages="{{item.image}}" data-vegetable_id="{{item.id}}" data-is_songcai="1"><image src="/images/circle-share.png"></image></button></view></view><view class="flex-justitem-spacecent" style="padding:10rpx 0;position: absolute;bottom: 60rpx;"><text class="title-sum">￥{{item.goods_price}}+{{item.goods_integral}} 积分</text></view></view></view></view></block></view>