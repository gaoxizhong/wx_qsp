.dou_anm{
  animation: dou_anm .3s linear infinite;
}
@keyframes dou_anm {
  0% {
    transform: rotate(-40deg)
  }
  25%{
    transform: rotate(0deg)
  }
  50% {
    transform: rotate(40deg)
  }
  75% {
    transform: rotate(0deg)
  }
  100%{
    transform: rotate(-40deg)

  }
}
